u0430\u043b\u0438\u0437\u0438\u0440\u0443\u044f \u0434\u043d\u0435\u0432\u043d\u0438\u043a\u043e\u0432\u044b\u0435 \u0437\u0430\u043c\u0435\u0442\u043a\u0438 \u043b\u044e\u0434\u0435\u0439 \u0438\u043d\u0442\u0435\u043b\u043b\u0435\u043a\u0442\u0443\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0442\u0440\u0443\u0434\u0430 \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e \u043e\u0442\u043c\u0435\u0447\u0430\u044f \u043d\u0430\u0441\u043b\u0435\u0434\u0438\u0435 \u043f\u0438\u0441\u0430\u0442\u0435\u043b\u0435\u0439-\u043d\u043e\u0432\u0430\u0442\u043e\u0440\u043e\u0432.\n[[/div]]\n\n[[div class=\"content-panel standalone series\"]]\n**11.03.19\u2588\u2588**\n\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u043b\u044e\u0434\u044f\u043c, \u043f\u043e\u0434\u0432\u0435\u0440\u0433\u043d\u0443\u0442\u044b\u043c SCP-1014, \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e 2-5 \u0434\u043d\u0435\u0439 \u043a\u043e\u043d\u0442\u0430\u043a\u0442\u0430 \u0441 \u0447\u0443\u0436\u043e\u0439 \u043d\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u043e\u0439 \u0440\u0435\u0447\u044c\u044e, \u043a\u0430\u043a \u0443\u0441\u0442\u043d\u043e\u0439, \u0442\u0430\u043a \u0438 \u043f\u0438\u0441\u044c\u043c\u0435\u043d\u043d\u043e\u0439, \u0434\u043b\u044f \u043b\u0438\u043a\u0432\u0438\u0434\u0430\u0446\u0438\u0438 \u044d\u0444\u0444\u0435\u043a\u0442\u0430 SCP-1014, \u043e\u0434\u043d\u0430\u043a\u043e **\u0431\u0435\u0441\u0435\u0434\u044b \u043d\u0435 \u0434\u043e\u043b\u0436\u043d\u044b \u043a\u0430\u0441\u0430\u0442\u044c\u0441\u044f SCP-1014, \u0444\u0438\u043b\u043e\u043b\u043e\u0433\u0438\u0438 \u0438 \u0441\u043c\u0435\u0436\u043d\u044b\u0445 \u043d\u0430\u0443\u043a**, \u0438\u043d\u0430\u0447\u0435 \u044d\u0442\u043e \u043c\u043e\u0436\u0435\u0442 \u043f\u0440\u0438\u0432\u0435\u0441\u0442\u0438 \u043a \u043d\u0435\u043f\u0440\u0435\u0434\u0432\u0438\u0434\u0435\u043d\u043d\u044b\u043c \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u0430\u043c.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u043a\u0430\u0436\u0435\u0442\u0441\u044f \u043b\u0438 SCP-1014-1, \u0447\u0442\u043e \u043e\u043d\u0438 \u043d\u0435 \u0438\u0441\u043f\u044b\u0442\u044b\u0432\u0430\u043b\u0438 \u0432\u043b\u0438\u044f\u043d\u0438\u044f SCP-1014, \u0438\u043b\u0438 \u043e\u043d\u0438 \u0441\u043a\u0440\u044b\u0432\u0430\u044e\u0442 \u0441\u0432\u043e\u0438 \u043f\u0435\u0440\u0435\u0436\u0438\u0432\u0430\u043d\u0438\u044f \u043d\u0430\u0441\u0447\u0451\u0442 SCP-1014, \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u0438\u0442\u044c \u0432 \u0434\u0430\u043b\u044c\u043d\u0435\u0439\u0448\u0438\u0445 \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u044f\u0445.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u0441\u0438\u043d\u0442\u0430\u043a\u0441\u0438\u0441 \u043f\u0440\u043e\u0438\u0437\u043d\u043e\u0441\u0438\u043c\u044b\u0445 \u0444\u0440\u0430\u0437 \u0446\u0435\u043b\u0438\u043a\u043e\u043c \u0432\u043e\u0437\u0432\u0440\u0430\u0449\u0430\u0435\u0442\u0441\u044f \u043a \u043d\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u043e\u043c\u0443 \u0441\u043f\u0443\u0441\u0442\u044f 7 \u0441\u0443\u0442\u043e\u043a.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u0441\u0443\u0449\u0435\u0441\u0442\u0432\u0443\u0435\u0442 \u0432\u0435\u0440\u043e\u044f\u0442\u043d\u043e\u0441\u0442\u044c, \u0447\u0442\u043e \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0434\u0438\u0442 \u0431\u043e\u043b\u0435\u0435 \u0441\u043a\u043e\u0440\u043e\u0435 \u0438\u0437\u0431\u0430\u0432\u043b\u0435\u043d\u0438\u0435 \u043e\u0442 SCP-1014, \u0435\u0441\u043b\u0438 SCP-1014-1 \u043e\u043a\u0440\u0443\u0436\u0430\u044e\u0442 \u043b\u044e\u0434\u0438 \u0441 \u0431\u043e\u0433\u0430\u0442\u043e\u0439 \u0440\u0435\u0447\u044c\u044e.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u043d\u043e \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u043e, \u0447\u0442\u043e SCP-1014-1, \u043d\u0430\u0445\u043e\u0434\u044f\u0449\u0438\u0435\u0441\u044f \u0432 \u0440\u0435\u0447\u0435\u0432\u043e\u0439 \u0438\u0437\u043e\u043b\u044f\u0446\u0438\u0438, \u0432\u0441\u043a\u043e\u0440\u043e\u0441\u0442\u0438 \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0442 \u043f\u0440\u043e\u044f\u0432\u043b\u044f\u0442\u044c \u044f\u0432\u043d\u044b\u0435 \u043f\u0440\u0438\u0437\u043d\u0430\u043a\u0438 \u043d\u0430\u0441\u0442\u0443\u043f\u0430\u044e\u0449\u0435\u0439 \u0434\u0435\u043c\u0435\u043d\u0446\u0438\u0438 (\u0438\u0434\u0435\u043d\u0442\u0438\u0444\u0438\u0446\u0438\u0440\u0443\u044e\u0442\u0441\u044f \u043a\u0430\u043a SCP-1014-2), \u0432 \u0442\u0435\u0447\u0435\u043d\u0438\u0435 2 \u043b\u0435\u0442 \u0434\u043e\u0445\u043e\u0434\u044f \u0434\u043e \u043f\u043e\u043b\u043d\u043e\u0439 \u0438\u043d\u0432\u0430\u043b\u0438\u0434\u043d\u043e\u0441\u0442\u0438.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u044d\u0442\u043e \u043f\u0440\u0438\u0432\u043e\u0434\u0438\u0442 \u043a \u0443\u0445\u0443\u0434\u0448\u0435\u043d\u0438\u044e \u0440\u0435\u0447\u0438 SCP-1014-2 \u0432\u043e \u0432\u0441\u0435\u0445 \u0430\u0441\u043f\u0435\u043a\u0442\u0430\u0445, \u0430 \u043d\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u0432 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u0438 \u043f\u043e\u0441\u0442\u0440\u043e\u0435\u043d\u0438\u044f \u0440\u0430\u0437\u043d\u043e\u043e\u0431\u0440\u0430\u0437\u043d\u044b\u0445 \u0440\u0430\u0437\u0432\u0451\u0440\u043d\u0443\u0442\u044b\u0445 \u0441\u0438\u043d\u0442\u0430\u043a\u0441\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440 \u0438 \u0441\u043e\u0431\u043b\u044e\u0434\u0435\u043d\u0438\u0438 \u0440\u0430\u0437\u043b\u0438\u0447\u043d\u044b\u0445 \u043f\u0443\u043d\u043a\u0442\u0443\u0430\u0446\u0438\u043e\u043d\u043d\u044b\u0445 \u043f\u0440\u0430\u0432\u0438\u043b.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u043d\u0435 \u043f\u0440\u0438\u0432\u0435\u043b\u0438 \u043a \u0437\u0430\u043c\u0435\u0442\u043d\u044b\u043c \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u0430\u043c \u0432 \u043e\u0442\u043d\u043e\u0448\u0435\u043d\u0438\u0438 SCP-1014-2 \u043f\u0440\u043e\u0442\u0435\u0441\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0435 \u043f\u043e \u043e\u0442\u0434\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \u0430\u043c\u043d\u0435\u0437\u0438\u0430\u043a\u0438 \u0438 \u043c\u043d\u0435\u0441\u0442\u0438\u043a\u0438.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u0442\u0430\u043a, \u0441\u043e\u0442\u0440\u0443\u0434\u043d\u0438\u043a\u0430\u043c \u0424\u043e\u043d\u0434\u0430 \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e \u0432 \u0431\u043b\u0438\u0436\u0430\u0439\u0448\u0435\u0435 \u0432\u0440\u0435\u043c\u044f \u0438\u0437\u0443\u0447\u0438\u0442\u044c \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044c \u043a\u043e\u043c\u043f\u043b\u0435\u043a\u0441\u043d\u043e\u0439 \u0442\u0435\u0440\u0430\u043f\u0438\u0438 \u0434\u043b\u044f SCP-1014-2, \u0432\u043a\u043b\u044e\u0447\u0430\u044e\u0449\u0435\u0439 \u043a\u043e\u043c\u0431\u0438\u043d\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0435 \u0441\u0445\u0435\u043c\u044b \u043f\u0440\u0438\u043c\u0435\u043d\u0435\u043d\u0438\u044f \u043f\u0440\u0435\u043f\u0430\u0440\u0430\u0442\u043e\u0432.\n[[/div]]\n\n[[div class=\"content-panel standalone series\"]]\n**12.03.19\u2588\u2588**\n\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u0421\u0438\u043d\u0442\u0430\u043a\u0441\u0438\u0441 SCP-1014-1, \u043f\u043e \u0432\u0441\u0435\u0439 \u0432\u0438\u0434\u0438\u043c\u043e\u0441\u0442\u0438, \u043f\u0440\u0435\u0442\u0435\u0440\u043f\u0435\u0432\u0430\u0435\u0442 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0435 \u0438\u0437\u043c\u0435\u043d\u0435\u043d\u0438\u0435 \u043f\u043e\u0441\u043b\u0435 \u0438\u0437\u0431\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u043e\u0442 \u044d\u0444\u0444\u0435\u043a\u0442\u0430 SCP-1014.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u041d\u0430\u043c \u0443\u0434\u0430\u043b\u043e\u0441\u044c, \u0438\u0441\u0445\u043e\u0434\u044f \u0438\u0437 \u0430\u043d\u0430\u043b\u0438\u0437\u0430 \u0434\u043d\u0435\u0432\u043d\u0438\u043a\u043e\u0432\u044b\u0445 \u0437\u0430\u043c\u0435\u0442\u043e\u043a \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0445 SCP-1014-1, \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u0438\u0442\u044c, \u0447\u0442\u043e \u044d\u0442\u0438 \u0441\u0443\u0431\u044a\u0435\u043a\u0442\u044b \u043f\u0440\u0438\u043e\u0431\u0440\u0435\u043b\u0438 \u0431\u043e\u043b\u0435\u0435 \u043f\u043b\u0430\u0432\u043d\u0443\u044e \u0438 \u0433\u0430\u0440\u043c\u043e\u043d\u0438\u0447\u043d\u043e \u0443\u0441\u0442\u0440\u043e\u0435\u043d\u043d\u0443\u044e \u0440\u0435\u0447\u044c, \u0434\u043e\u0441\u0442\u0438\u0433\u043d\u0443\u0432 \u043b\u0443\u0447\u0448\u0435\u0433\u043e \u0432\u043b\u0430\u0434\u0435\u043d\u0438\u044f \u0440\u043e\u0434\u043d\u044b\u043c \u044f\u0437\u044b\u043a\u043e\u043c, \u0447\u0435\u043c \u0434\u043e \u043a\u043e\u043d\u0442\u0430\u043a\u0442\u0430 \u0441 SCP-1014.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u041a\u0430\u0436\u0435\u0442\u0441\u044f \u043b\u0438 \u0438\u043c, \u0447\u0442\u043e \u043e\u043d\u0438 \u043f\u043e\u0441\u0442\u0438\u0433\u043b\u0438 \u044d\u0442\u043e \u0441\u0430\u043c\u043e\u0441\u0442\u043e\u044f\u0442\u0435\u043b\u044c\u043d\u043e, \u0438\u043b\u0438 \u0436\u0435 \u043e\u043d\u0438 \u0447\u0451\u0442\u043a\u043e \u043e\u0441\u043e\u0437\u043d\u0430\u044e\u0442 \u043c\u043e\u043c\u0435\u043d\u0442, \u043a\u043e\u0433\u0434\u0430 \u0438\u0445 \u0432\u043b\u0430\u0434\u0435\u043d\u0438\u0435 \u0440\u0435\u0447\u044c\u044e \u043d\u0430\u0447\u0430\u043b\u043e \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e \u0443\u043b\u0443\u0447\u0448\u0430\u0442\u044c\u0441\u044f, \u0438\u0437 \u043f\u043e\u043b\u0443\u0447\u0435\u043d\u043d\u044b\u0445 \u043d\u0430\u043c\u0438 \u0441\u0432\u0435\u0434\u0435\u043d\u0438\u0439 \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u0438\u0442\u044c \u043d\u0435\u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u041f\u0440\u0438\u0432\u044b\u0447\u043d\u044b\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c SCP-1014-1 \u0441\u043c\u043e\u0433\u0443\u0442 \u0432\u044b\u0440\u0430\u0436\u0430\u0442\u044c\u0441\u044f \u043f\u043e\u0441\u043b\u0435 \u043e\u043a\u043e\u043d\u0447\u0430\u043d\u0438\u044f \u0432\u043e\u0437\u0434\u0435\u0439\u0441\u0442\u0432\u0438\u044f SCP-1014 \u0442\u0430\u043a \u0436\u0435 \u0445\u043e\u0440\u043e\u0448\u043e, \u043a\u0430\u043a \u0438 \u0441\u043f\u043e\u0441\u043e\u0431\u043e\u043c, \u043a\u043e\u0442\u043e\u0440\u044b\u043c \u0432\u044b\u0440\u0430\u0436\u0435\u043d\u044b \u0442\u0435\u043a\u0441\u0442\u044b, \u0434\u0435\u043c\u043e\u043d\u0441\u0442\u0440\u0438\u0440\u0443\u044e\u0449\u0438\u0435 SCP-1014, \u0438\u043c\u0438\u0442\u0438\u0440\u0443\u044f \u0435\u0433\u043e \u0432 \u0445\u0443\u0434\u043e\u0436\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439 \u0444\u043e\u0440\u043c\u0435.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u041d\u043e \u0441\u0442\u043e\u0438\u0442 \u043e\u0442\u043c\u0435\u0442\u0438\u0442\u044c, \u0447\u0442\u043e \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438 \u0438\u0441\u043a\u043b\u044e\u0447\u0435\u043d\u0430 \u0432 \u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0445 \u0440\u0435\u0430\u043b\u0438\u044f\u0445 \u0432\u0435\u0440\u043e\u044f\u0442\u043d\u043e\u0441\u0442\u044c \u043d\u0430\u0445\u043e\u0436\u0434\u0435\u043d\u0438\u044f \u043b\u044e\u0434\u0435\u0439 \u0432 \u0440\u0435\u0447\u0435\u0432\u043e\u0439 \u0438\u0437\u043e\u043b\u044f\u0446\u0438\u0438, \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0435\u043d\u043d\u043e, \u0430\u0431\u0441\u043e\u043b\u044e\u0442\u043d\u043e\u0435 \u0431\u043e\u043b\u044c\u0448\u0438\u043d\u0441\u0442\u0432\u043e SCP-1014-1 \u043f\u0440\u0438\u0434\u0443\u0442 \u043a \u043d\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u043e\u043c\u0443 \u0434\u043e-\u043a\u043e\u043d\u0442\u0430\u043a\u0442\u043d\u043e\u043c\u0443 \u0441\u043e\u0441\u0442\u043e\u044f\u043d\u0438\u044e \u0435\u0441\u0442\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u043c \u043f\u0443\u0442\u0451\u043c.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u041c\u044b \u043d\u0430 \u0442\u0435\u043a\u0443\u0449\u0438\u0439 \u043c\u043e\u043c\u0435\u043d\u0442 \u043d\u0435 \u043c\u043e\u0436\u0435\u043c \u043e\u0442\u0441\u043b\u0435\u0434\u0438\u0442\u044c \u043f\u0443\u0442\u0438 \u0438 \u0441\u043f\u043e\u0441\u043e\u0431\u044b \u0440\u0430\u0441\u043f\u0440\u043e\u0441\u0442\u0440\u0430\u043d\u0435\u043d\u0438\u044f SCP-1014 \u0434\u043e\u0441\u0442\u043e\u0432\u0435\u0440\u043d\u043e.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u0415\u0433\u043e \u043d\u0430\u0438\u0431\u043e\u043b\u0435\u0435 \u0432\u0435\u0440\u043e\u044f\u0442\u043d\u044b\u0439 \u0441\u043f\u043e\u0441\u043e\u0431 \u043f\u0440\u0435\u0434\u044a\u044f\u0432\u043b\u0435\u043d\u0438\u044f \u2014 \u0432\u043e\u043f\u043b\u043e\u0449\u0435\u043d\u0438\u0435 \u00ab\u0431\u0435\u0434\u043d\u043e\u0433\u043e\u00bb \u0441\u0438\u043d\u0442\u0430\u043a\u0441\u0438\u0441\u0430 (\u0441 \u043f\u043e\u0447\u0442\u0438 \u0446\u0435\u043b\u0438\u043a\u043e\u043c \u043e\u0442\u0441\u0443\u0442\u0441\u0442\u0432\u0443\u044e\u0449\u0435\u0439 \u043f\u0443\u043d\u043a\u0442\u0443\u0430\u0446\u0438\u0435\u0439) \u0432 \u043e\u0441\u043d\u043e\u0432\u0435 \u043e\u0431\u0440\u0430\u0437\u0446\u043e\u0432 \u00ab\u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u043e\u0433\u043e\u00bb \u00ab\u0430\u0432\u0430\u043d\u0433\u0430\u0440\u0434\u043d\u043e\u0433\u043e\u00bb \u0432\u0435\u0440\u0431\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0438\u0441\u043a\u0443\u0441\u0441\u0442\u0432\u0430.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u041d\u0435 \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u043e \u0442\u0430\u043a\u0436\u0435, \u0441\u043b\u0443\u0447\u0430\u0439\u043d\u044b \u043b\u0438 \u044d\u0442\u0438 \u043f\u0440\u0435\u0434\u044a\u044f\u0432\u043b\u0435\u043d\u0438\u044f \u043a\u0430\u043a \u0440\u0430\u0441\u043f\u0440\u043e\u0441\u0442\u0440\u0430\u043d\u0435\u043d\u0438\u0435 \u043f\u043e\u0434\u0442\u0438\u043f\u0430 \u00ab\u0430\u0432\u0430\u043d\u0433\u0430\u0440\u0434\u043d\u043e\u0439\u00bb \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u044b, \u0441\u043e\u0437\u0434\u0430\u043d\u043d\u043e\u0439 \u0441\u0430\u043c\u0438\u043c\u0438 SCP-1014-1 (\u0432\u043e \u0432\u0440\u0435\u043c\u044f \u0438\u043b\u0438 \u043f\u043e\u0441\u043b\u0435 \u0432\u043e\u0437\u0434\u0435\u0439\u0441\u0442\u0432\u0438\u044f SCP-1014), \u0438\u043b\u0438 \u0436\u0435 \u0442\u0430\u043a\u0438\u0435 \u0442\u0435\u043a\u0441\u0442\u044b \u0440\u0430\u0441\u043f\u0440\u043e\u0441\u0442\u0440\u0430\u043d\u044f\u044e\u0442\u0441\u044f \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u044c\u043d\u043e \u043d\u0430 \u0444\u043e\u0440\u0443\u043c\u0430\u0445 \u0438 \u0441\u0430\u0439\u0442\u0430\u0445 \u0441 \u00ab\u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u043c\u00bb \u0438\u0441\u043a\u0443\u0441\u0441\u0442\u0432\u043e\u043c \u043d\u0435\u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u043d\u044b\u043c \u0432 \u0442\u0435\u043a\u0443\u0449\u0438\u0439 \u043c\u043e\u043c\u0435\u043d\u0442 \u043a\u0440\u0443\u0433\u043e\u043c \u043b\u0438\u0446.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u0417\u043d\u0430\u0435\u043c \u043d\u0430 \u0434\u0430\u043d\u043d\u043e\u043c \u044d\u0442\u0430\u043f\u0435 \u043c\u044b \u0442\u043e\u043b\u044c\u043a\u043e \u0442\u043e, \u0447\u0442\u043e \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e \u0441\u0444\u043e\u0440\u043c\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0433\u0440\u0443\u043f\u043f\u0443 \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u0438\u0441\u0442\u043e\u0432 \u0424\u043e\u043d\u0434\u0430, \u043f\u0440\u0438\u0437\u0432\u0430\u043d\u043d\u0443\u044e \u0440\u0435\u0448\u0438\u0442\u044c \u0432\u0441\u0435 \u043e\u0442\u043a\u0440\u044b\u0442\u044b\u0435 \u0432\u043e\u043f\u0440\u043e\u0441\u044b, \u0433\u043b\u0430\u0432\u043d\u044b\u0439 \u0438\u0437 \u043a\u043e\u0442\u043e\u0440\u044b\u0445: \u0430 \u043d\u0443\u0436\u043d\u043e \u043b\u0438 \u0432\u043e\u043e\u0431\u0449\u0435 \u0447\u0442\u043e-\u043b\u0438\u0431\u043e \u0434\u0435\u043b\u0430\u0442\u044c \u0441 SCP-1014-1, \u043d\u0435 \u043d\u0430\u0445\u043e\u0434\u044f\u0449\u0438\u043c\u0438\u0441\u044f \u0432 \u0440\u0435\u0447\u0435\u0432\u043e\u0439 \u0438\u0437\u043e\u043b\u044f\u0446\u0438\u0438, \u0438 \u0441 SCP-1014-2, \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u043f\u043e\u043a\u0430 \u043d\u0435\u0442 \u043d\u0438\u043a\u0430\u043a\u043e\u0433\u043e \u0441\u043f\u043e\u0441\u043e\u0431\u0430 \u0438\u0437\u043b\u0435\u0447\u0438\u0442\u044c?\n[[/div]]\n\n[[div class=\"content-panel standalone series\"]]\n**26.03.19\u2588\u2588**\n\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u041f\u043e\u043c\u043e\u0433\u0438\u0442\u0435! --\u043c\u043d\u0435-- \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u0438\u0442\u044c, \u0447\u0442\u043e \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0434\u0438\u0442 \u043d\u0430 \u0441\u0430\u043c\u043e\u043c \u0434\u0435\u043b\u0435, \u0435\u0441\u043b\u0438 \u0447\u0438\u0442\u0430\u0435\u0442\u0435 \u044d\u0442\u043e\u0442 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442!\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u041c\u043d\u0435! \u043d\u0435 \u0443\u0434\u0430\u043b\u043e\u0441\u044c \u0432\u044b\u043f\u043e\u043b\u043d\u0438\u0442\u044c --\u044d\u0442\u0443-- \u043f\u043e\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043d\u0443\u044e \u043f\u0435\u0440\u0435\u0434\u043e \u043c\u043d\u043e\u044e \u0437\u0430\u0434\u0430\u0447\u0443; \u044f \u043f\u044b\u0442\u0430\u043b\u0441\u044f, \u043d\u043e \u0447\u0442\u043e-\u0442\u043e \u043d\u0435 \u0434\u0430\u0432\u0430\u043b\u043e --\u043c\u043d\u0435-- \u0434\u043e\u0432\u0435\u0441\u0442\u0438 \u0434\u0435\u043b\u043e \u0434\u043e \u043a\u043e\u043d\u0446\u0430, \u0431\u0443\u0434\u0442\u043e \u043a\u0430\u043a\u0430\u044f-\u0442\u043e \u043c\u044b\u0441\u043b\u044c \u0432\u0435\u0440\u0442\u0435\u043b\u0430\u0441\u044c \u043d\u0430 --\u0433\u0440\u0430\u043d\u0438\u0446\u0435-- \u043a\u0440\u0430\u044e \u0441\u043e\u0437\u043d\u0430\u043d\u0438\u044f, \u043d\u043e \u044f \u043d\u0435 \u043c\u043e\u0433 \u0437\u0430 \u043d\u0435\u0451 \u0443\u0445\u0432\u0430\u0442\u0438--\u0446\u0430--\u0442\u044c\u0441\u044f--; \u044f \u0438 \u044d\u0442\u043e\u0442-\u0442\u043e \u0442\u0435\u043a\u0441\u0442 \u0435\u043b\u0435 \u0441\u043c\u043e\u0433 \u0434\u043e\u043f\u0438\u0441\u0430\u0442\u044c \u0437\u0430 \u0434\u0432\u0435 \u043d\u0435\u0434\u0435\u043b\u0438: \u0441\u043b\u043e\u0432\u0430 \u0442\u044f\u0436\u0435\u043b\u043e \u0441\u043a\u043b\u0430\u0434\u044b\u0432\u0430\u044e\u0442\u0441\u044f \u0432\u043c\u0435\u0441\u0442\u0435--...\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@--\u041f\u0440\u0435\u043a\u0440\u0430\u0442\u0438\u0442\u044c! \u0434\u0443\u043c\u0430\u0442\u044c \u043e \u043d\u0435\u0439 \u0431\u044b\u043b\u043e \u043d\u0435\u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e, \u043f\u043e\u044d\u0442\u043e\u043c\u0443-- \u044f \u043f\u0435\u0440\u0435\u0434\u0430\u044e \u0432\u0430\u043c \u0441\u0432\u043e\u0438 \u0447\u0435\u0440\u043d\u043e\u0432\u0438\u043a\u0438 \u043e\u0431 \u043e\u0431\u044a\u0435\u043a\u0442\u0435 \u0438 \u043f\u0440\u043e\u0448\u0443 \u043c\u0435\u043d\u044f \u043e\u0442\u0441\u0442\u0440\u0430\u043d\u0438\u0442\u044c \u043e\u0442 \u0440\u0430\u0431\u043e\u0442\u044b \u043f\u043e \u0435\u0433\u043e \u043e\u043f\u0438\u0441\u0430\u043d\u0438\u044e.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@--\u0422\u0430\u043a! \u044f \u0434\u0443\u043c\u0430\u044e, \u0431\u0443\u0434\u0435\u0442 \u043b\u0443\u0447\u0448\u0435 \u043d\u0430\u043c \u0432\u0441\u0435\u043c, \u043f\u043e\u0442\u043e\u043c\u0443 \u0447\u0442\u043e-- \u043e\u0442 \u043c\u0435\u043d\u044f, \u043a\u0430\u0436\u0435\u0442\u0441\u044f, \u043d\u0435 \u0431\u0443\u0434\u0435\u0442 \u0431\u043e\u043b\u044c\u0448\u0435 \u043d\u0438\u043a\u0430\u043a\u043e\u0433\u043e \u0442\u043e\u043b\u043a\u0430.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u0420\u0430\u0437\u0433\u043e\u0432\u0430\u0440\u0438\u0432\u0430\u0442\u044c! \u0441 \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0441\u0442\u0432\u043e\u043c --\u0431\u0435\u0441\u0442\u043e\u043b\u043a\u0443-- \u044f \u043d\u0435 \u0445\u043e\u0447\u0443 \u2014 \u0437\u043d\u0430\u044e, \u0447\u0442\u043e \u043e\u043d\u043e \u043c\u043d\u0435 \u0441\u043a\u0430\u0436\u0435\u0442: \u043f\u043e\u0434\u0432\u0451\u043b, \u043d\u0435 \u0441\u043f\u0440\u0430\u0432\u0438\u043b\u0441\u044f \u0438 \u043f\u0440\u043e\u0447\u0435\u0435; \u0442\u0430\u043a \u0447\u0442\u043e \u044f \u043f\u0440\u043e\u0441\u0442\u043e \u043e\u0441\u0442\u0430\u0432\u043b\u044e \u044d\u0442\u0443 \u0442\u0435\u0442\u0440\u0430\u0434\u044c \u043d\u0430 \u0441\u0442\u043e\u043b\u0435.\n@@\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0@@\u041f\u043e\u0436\u0430\u043b\u0443\u0439\u0441\u0442\u0430! //\u0431\u0443\u0434\u044c\u0442\u0435 \u0431\u043b\u0430\u0433\u043e\u0440\u0430\u0437\u0443\u043c\u043d\u044b//, \u0432\u0435\u0434\u044c \u044f \u0438\u0441\u043a\u0440\u0435\u043d\u043d\u0435 \u043d\u0430\u0434\u0435\u044e\u0441\u044c, \u0447\u0442\u043e \u043d\u0430\u0439\u0434\u0451\u0442\u0441\u044f \u043a\u0442\u043e-\u0442\u043e \u043f\u043e\u043b\u0443\u0447\u0448\u0435 \u043c\u0435\u043d\u044f--, \u043d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, \u0442\u044b, \u0416\u2588\u2588 \u0414\u0435\u2588\u2588\u2588\u2588--.\n[[/div]]\n\n[[footnoteblock title=\"\u0421\u043d\u043e\u0441\u043a\u0438\"]]\n\n----\n= [[size 90%]]**<< [[[SCP-1013-RU]]] | SCP-1014-RU | [[[SCP-1015-RU]]] >>**[[/size]]", "tags": ["\u0430\u043d\u043e\u043c\u0430\u043b\u0438\u044f:\u043b\u0438\u043d\u0433\u0432\u0438\u0441\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0439", "\u0430\u043d\u043e\u043c\u0430\u043b\u0438\u044f:\u043c\u0435\u043c\u0435\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0439", "\u043a\u043b\u0430\u0441\u0441:\u0432\u043d\u0435\u0441\u0438\u0441\u0442\u0435\u043c\u043d\u044b\u0439", "\u043e\u0442\u0434\u0435\u043b:\u043e\u043f\u043d", "\u0441\u0432\u043e\u0439\u0441\u0442\u0432\u043e:\u0438\u0441\u043a\u0443\u0441\u0441\u0442\u0432\u043e", "\u0441\u043e\u0431\u044b\u0442\u0438\u0435:\u043f\u0440\u043e\u0442\u0435\u0433\u0430\u043d\u043e", "\u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0430:_\u043e\u0434\u043e\u0431\u0440\u0435\u043d\u043e-\u0430\u043c\u0441", "\u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0430:\u043e\u0431\u044a\u0435\u043a\u0442", "\u0442\u0435\u043c\u0430\u0442\u0438\u043a\u0430:\u043d\u0435_\u0441\u043e\u0434\u0435\u0440\u0436\u0430\u0449\u0438\u0439\u0441\u044f", "\u0442\u0438\u043f:\u043a\u043e\u0440\u043e\u0442\u043a\u0438\u0439", "\u0442\u0438\u043f:\u043d\u0435\u0444\u043e\u0440\u043c\u0430\u0442", "\u0444\u0438\u043b\u0438\u0430\u043b:ru"], "parent": null, "locked": false}