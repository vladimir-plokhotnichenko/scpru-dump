45 \u0437\u0430\u043b\u043e\u0436\u0435\u043d\u043d\u044b\u0435, \u043d\u0435 \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u0434\u043e\u043b\u0436\u043d\u044b \u0431\u044b\u0442\u044c \"\u0433\u043b\u0443\u0431\u043e\u043a\u0438\u043c\u0438\" \u0438\u043b\u0438 \u0442\u0449\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u043f\u0440\u043e\u0434\u0443\u043c\u0430\u043d\u043d\u044b\u043c\u0438, \u0433\u043b\u0430\u0432\u043d\u043e\u0435 -- \u043e\u0441\u0442\u0430\u0432\u0438\u0442\u044c \u0447\u0438\u0442\u0430\u0442\u0435\u043b\u044f \u0441 \u0447\u0435\u043c-\u0442\u043e, \u0447\u0442\u043e \u0435\u0433\u043e \u0443\u0434\u0438\u0432\u0438\u0442. \u041d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, \u0432\u043e\u043f\u0440\u043e\u0441 \u0432 \u0441\u0442\u0438\u043b\u0435 \"\u0410 \u0431\u044b\u043b\u043e \u043b\u0438 \u044d\u0442\u043e \u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u043e?\" \u0438\u043b\u0438 \"\u0410 \u0447\u0442\u043e \u0435\u0449\u0451 \u043e\u043d\u0438 \u043c\u043e\u0433\u043b\u0438 \u0441\u0434\u0435\u043b\u0430\u0442\u044c?\". \u0418\u0441\u0442\u043e\u0440\u0438\u0438 \u0434\u043e\u043b\u0436\u043d\u044b \u0431\u044b\u0442\u044c \u043a\u043e\u0440\u043e\u0442\u043a\u0438\u043c\u0438, \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u0442\u044c \u0443\u043c\u0435\u0440\u0435\u043d\u043d\u044b\u0439 \u044e\u043c\u043e\u0440 \u0438\u043b\u0438 \u0434\u0440\u0430\u043c\u0443 \u0438, \u043a\u0430\u043a \u043b\u044e\u0431\u0430\u044f \u0445\u043e\u0440\u043e\u0448\u0430\u044f \u0438\u0441\u0442\u043e\u0440\u0438\u044f, \u0434\u043e\u043b\u0436\u043d\u044b \u043e\u0441\u0442\u0430\u0432\u0430\u0442\u044c\u0441\u044f \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u044b\u043c\u0438 \u043d\u0430 \u0432\u0441\u0451\u043c \u0441\u0432\u043e\u0451\u043c \u043f\u0440\u043e\u0442\u044f\u0436\u0435\u043d\u0438\u0438, \u0447\u0442\u043e\u0431\u044b \u0447\u0438\u0442\u0430\u0442\u0435\u043b\u044e \u043d\u0435 \u0445\u043e\u0442\u0435\u043b\u043e\u0441\u044c \u043f\u0440\u043e\u0441\u0442\u043e \u043f\u0440\u043e\u0431\u0435\u0436\u0430\u0442\u044c \u0438\u0445 \u0433\u043b\u0430\u0437\u0430\u043c\u0438. \u0418\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u043d\u0435 \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u044b, \u043d\u043e \u0442\u043e\u0447\u043d\u043e \u0434\u043e\u043f\u0443\u0441\u0442\u0438\u043c\u044b.\n\n\u0415\u0449\u0451 \u043e\u0434\u043d\u0430 \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e\u0441\u0442\u044c \u0414\u043d\u0435\u0432\u043d\u0438\u043a\u0430 \u0441\u043e\u0441\u0442\u043e\u0438\u0442 \u0432 \u0442\u043e\u043c, \u0447\u0442\u043e \u043e\u043d \u043e\u0431\u044b\u0447\u043d\u043e \u043f\u043e\u0432\u0435\u0441\u0442\u0432\u0443\u0435\u0442 \u043d\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u043e\u0431 \u0430\u0432\u0442\u043e\u0440\u0435 \u044d\u0442\u0438\u0445 \u0437\u0430\u043f\u0438\u0441\u0435\u0439, \u043d\u043e \u0438 \u043e \u0434\u0440\u0443\u0433\u0438\u0445 \u043b\u044e\u0434\u044f\u0445. \u041d\u0435\u0432\u0430\u0436\u043d\u043e, \u0441\u0438\u043b\u044c\u043d\u044b\u0439 \u043b\u0438 \u044d\u0442\u043e \u0433\u0438\u0433\u0430\u043d\u0442 \u0438\u043b\u0438 \u043a\u043e\u0440\u043e\u043b\u0435\u0432\u0430 \u043f\u0443\u0441\u0442\u043e\u0448\u0438, \u043a\u043e\u043d\u0444\u043b\u0438\u043a\u0442 \u043c\u0435\u0436\u0434\u0443 \u0430\u0432\u0442\u043e\u0440\u043e\u043c \u0438 \u043a\u0435\u043c-\u043d\u0438\u0431\u0443\u0434\u044c \u0435\u0449\u0451 \u043d\u0438\u043a\u043e\u0433\u0434\u0430 \u043d\u0435 \u0441\u0442\u0430\u043d\u043e\u0432\u0438\u0442\u0441\u044f \u0446\u0435\u043d\u0442\u0440\u043e\u043c \u0438\u0441\u0442\u043e\u0440\u0438\u0438. \u0412\u043c\u0435\u0441\u0442\u043e \u044d\u0442\u043e\u0433\u043e \u0430\u043a\u0446\u0435\u043d\u0442 \u0434\u0435\u043b\u0430\u0435\u0442\u0441\u044f \u043d\u0430 \u0447\u0443\u0434\u0435\u0441\u0430\u0445 \u0438 \u0442\u0440\u0430\u0433\u0435\u0434\u0438\u044f\u0445 \u0434\u0430\u0432\u043d\u043e \u043c\u0438\u043d\u0443\u0432\u0448\u0438\u0445, \u043e \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u044b\u0432\u0430\u044e\u0442 \u0441\u0442\u0440\u0430\u043d\u043d\u0438\u043a\u0443. \u042d\u0442\u043e \u043d\u0438 \u0432 \u043a\u043e\u0435\u043c \u0441\u043b\u0443\u0447\u0430\u0435 \u043d\u0435 \u043e\u0433\u0440\u0430\u043d\u0438\u0447\u0435\u043d\u0438\u0435 -- \u043c\u043e\u0436\u0435\u0442\u0435 \u0441\u043f\u043e\u043a\u043e\u0439\u043d\u043e \u0432\u0432\u043e\u0434\u0438\u0442\u044c \u043d\u043e\u0442\u043a\u0438 \u0438\u043d\u043e\u043c\u0435\u0440\u043d\u043e\u0433\u043e, \u0447\u0435\u0433\u043e-\u0442\u043e, \u043d\u0430 \u0447\u0442\u043e \u0430\u0432\u0442\u043e\u0440 \u043c\u043e\u0433 \u0431\u044b \u0441\u0440\u0435\u0430\u0433\u0438\u0440\u043e\u0432\u0430\u0442\u044c, \u043d\u043e \u043f\u043e\u043c\u043d\u0438\u0442\u0435, \u0447\u0442\u043e \u0430\u0432\u0442\u043e\u0440 \u043e\u0431\u044b\u0447\u043d\u043e \u043d\u0435 \u0432\u044b\u0440\u0430\u0436\u0430\u0435\u0442 \u0441\u0432\u043e\u0435\u0433\u043e \u043c\u043d\u0435\u043d\u0438\u044f.\n\n\u041e\u0434\u043d\u0430\u043a\u043e \u043d\u0435 \u0441\u0442\u043e\u0438\u0442 \u0437\u0430\u0431\u044b\u0432\u0430\u0442\u044c \u0438 \u0442\u043e, \u043a\u0435\u043c \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0430\u0432\u0442\u043e\u0440. \u0415\u0441\u043b\u0438 \u0432\u044b \u043d\u0435 \u0437\u0430\u043c\u0435\u0442\u0438\u043b\u0438, \u0435\u0441\u0442\u044c \u043e\u043f\u0440\u0435\u0434\u0435\u043b\u0451\u043d\u043d\u044b\u0435 \u043c\u043e\u0442\u0438\u0432\u044b[[footnote]]\u041c\u043e\u0442\u0438\u0432\u044b -- \u044d\u0442\u043e \u043f\u043e\u0432\u0442\u043e\u0440\u044f\u044e\u0449\u0438\u0435\u0441\u044f \u0437\u043d\u0430\u043a\u0438, \u0445\u0430\u0440\u0430\u043a\u0442\u0435\u0440\u0438\u0441\u0442\u0438\u043a\u0438 \u0438\u043b\u0438 \u0441\u0438\u043c\u0432\u043e\u043b\u044b, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043f\u043e\u0441\u0442\u043e\u044f\u043d\u043d\u043e \u0432\u043e\u0437\u043d\u0438\u043a\u0430\u044e\u0442 \u043d\u0430 \u043f\u0440\u043e\u0442\u044f\u0436\u0435\u043d\u0438\u0438 \u0446\u0438\u043a\u043b\u0430 \u0438\u043b\u0438 \u0438\u0441\u0442\u043e\u0440\u0438\u0438. \u041e\u043d\u0438 \u043c\u043e\u0433\u0443\u0442 \u0431\u044b\u0442\u044c \u043e\u0447\u0435\u043d\u044c \u0437\u043d\u0430\u0447\u0438\u043c\u044b\u043c\u0438, \u0438\u043b\u0438 \u043f\u043e\u0447\u0442\u0438 \u043d\u0435 \u0438\u043c\u0435\u0442\u044c \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u044f \u0434\u043b\u044f \u0438\u0441\u0442\u043e\u0440\u0438\u0438 -- \u0432\u0441\u0451 \u0437\u0430\u0432\u0438\u0441\u0438\u0442 \u043e\u0442 \u043f\u0438\u0441\u0430\u0442\u0435\u043b\u044f.[[/footnote]]: \u043b\u0435\u0441\u0430, \u0440\u0430\u0441\u0442\u0435\u043d\u0438\u044f \u0438 \u0441\u0430\u043c\u0430 \u043f\u043e \u0441\u0435\u0431\u0435 \u0434\u0438\u043a\u0430\u044f \u043f\u0440\u0438\u0440\u043e\u0434\u0430 \u0438\u0433\u0440\u0430\u044e\u0442 \u0431\u043e\u043b\u044c\u0448\u0443\u044e \u0440\u043e\u043b\u044c, \u0432 \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e\u0441\u0442\u0438 \u043f\u0442\u0438\u0446\u044b, \u043d\u0430 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0430\u0432\u0442\u043e\u0440 \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0440\u0430\u0437 \u0441\u0441\u044b\u043b\u0430\u0435\u0442\u0441\u044f. \u0422\u0430\u043a\u0436\u0435 \u0432 \u0438\u0441\u0442\u043e\u0440\u0438\u044f\u0445 \u0432\u0441\u0435\u0433\u0434\u0430 \u043f\u0440\u0438\u0441\u0443\u0442\u0441\u0442\u0432\u0443\u0435\u0442 \u0434\u0443\u0445 \u0441\u0442\u0440\u0430\u043d\u0441\u0442\u0432\u0438\u044f \u0438 \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u0439. \u041d\u0430\u043a\u043e\u043d\u0435\u0446, \u0430\u0432\u0442\u043e\u0440 \u043e\u0431\u044b\u0447\u043d\u043e \u043f\u043e\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442\u0441\u044f \u043a\u0430\u043a \u0434\u043e\u0431\u0440\u043e\u0441\u0435\u0440\u0434\u0435\u0447\u043d\u044b\u0439 \u0441\u0442\u0440\u0430\u043d\u043d\u0438\u043a, \u0430 \u043d\u0435 \u044f\u0440\u044b\u0439 \u0431\u043e\u0440\u0435\u0446 \u0437\u0430 \u0432\u044b\u0436\u0438\u0432\u0430\u043d\u0438\u0435[[footnote]]\u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u043f\u043e\u0434\u0443\u043c\u0430\u0439\u0442\u0435 \u0434\u0432\u0430\u0436\u0434\u044b, \u043f\u0440\u0435\u0436\u0434\u0435 \u0447\u0435\u043c \u0434\u043e\u0431\u0430\u0432\u043b\u044f\u0442\u044c \u0432 \u0438\u0441\u0442\u043e\u0440\u0438\u044e \u0437\u0440\u0435\u043b\u0438\u0449\u043d\u0443\u044e \u0431\u0438\u0442\u0432\u0443 \u0438\u043b\u0438 \u0431\u0435\u0441\u0441\u0435\u0440\u0434\u0435\u0447\u043d\u0443\u044e \u0440\u0435\u0437\u043d\u044e. \u0415\u0441\u043b\u0438 \u0432\u044b \u0445\u043e\u0442\u0438\u0442\u0435 \u043f\u0438\u0441\u0430\u0442\u044c \u0442\u0430\u043a\u043e\u0435 -- \u044d\u0442\u043e \u0421\u043e\u0431\u0440\u0430\u043d\u0438\u0435 \u043d\u0435 \u0434\u043b\u044f \u0432\u0430\u0441.[[/footnote]].\n\n\u0421\u043e\u0432\u0435\u0442 \u043d\u0430\u043f\u043e\u0441\u043b\u0435\u0434\u043e\u043a: \u043d\u0435 \u0431\u043e\u0439\u0442\u0435\u0441\u044c \u043f\u0440\u043e\u0441\u0438\u0442\u044c \u043a\u0440\u0438\u0442\u0438\u043a\u0438 \u043d\u0430 \u0444\u043e\u0440\u0443\u043c\u0435 \u0438\u043b\u0438 \u0432 \u0447\u0430\u0442\u0430\u0445. \u0414\u0440\u0443\u0433\u0438\u0435, \u0431\u043e\u043b\u0435\u0435 \u043e\u043f\u044b\u0442\u043d\u044b\u0435 \u043f\u0438\u0441\u0430\u0442\u0435\u043b\u0438, \u043b\u0443\u0447\u0448\u0435 \u0432\u0441\u0435\u0433\u043e \u0441\u043c\u043e\u0433\u0443\u0442 \u043f\u043e\u043c\u043e\u0447\u044c \u0441 \u0441\u043e\u0445\u0440\u0430\u043d\u0435\u043d\u0438\u0435\u043c \u0441\u0443\u0449\u043d\u043e\u0441\u0442\u0438 \u0414\u043d\u0435\u0432\u043d\u0438\u043a\u0430. \u041f\u0440\u0438 \u043f\u0443\u0431\u043b\u0438\u043a\u0430\u0446\u0438\u0438, \u0432\u0441\u0435 \u0437\u0430\u043f\u0438\u0441\u0438 \u0434\u043e\u043b\u0436\u043d\u044b \u0438\u043c\u0435\u0442\u044c \u0442\u0435\u0433 [https://scpfoundation.net/system:page-tags/tag/\u0431\u0438\u0431\u043b\u0438\u043e\u0442\u0435\u043a\u0430:\u0434\u043d\u0435\u0432\u043d\u0438\u043a_\u0438\u0434\u0443\u0449\u0435\u0433\u043e \u0431\u0438\u0431\u043b\u0438\u043e\u0442\u0435\u043a\u0430:\u0434\u043d\u0435\u0432\u043d\u0438\u043a_\u0438\u0434\u0443\u0449\u0435\u0433\u043e] \u0438 \u043d\u0430\u0447\u0438\u043d\u0430\u0442\u044c\u0441\u044f \u0442\u0430\u043a:\n\n> @@ +++ \u0414\u043d\u0435\u0432\u043d\u0438\u043a \u0438\u0434\u0443\u0449\u0435\u0433\u043e, [\u0414\u0435\u043d\u044c \u043d\u0435\u0434\u0435\u043b\u0438], [\u0427\u0438\u0441\u043b\u043e \u0438 \u043c\u0435\u0441\u044f\u0446] @@\n\n\u041a\u043e\u0433\u0434\u0430 \u0432\u044b \u043e\u043f\u0443\u0431\u043b\u0438\u043a\u043e\u0432\u0430\u043b\u0438 \u043d\u043e\u0432\u0443\u044e \u0437\u0430\u043f\u0438\u0441\u044c \u0432 \u043e\u0441\u043d\u043e\u0432\u043d\u043e\u0435 \u043f\u0440\u043e\u0441\u0442\u0440\u0430\u043d\u0441\u0442\u0432\u043e, \u0441\u043c\u0435\u043b\u043e \u0434\u043e\u0431\u0430\u0432\u043b\u044f\u0439\u0442\u0435 \u0435\u0451 \u0432 \"\u0412\u043e\u0441\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u043d\u044b\u0435 \u0437\u0430\u043f\u0438\u0441\u0438\". \u0421\u043b\u0435\u0434\u0443\u0439\u0442\u0435 \u0444\u043e\u0440\u043c\u0430\u0442\u0443, \u043d\u0430\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u043c\u043e\u0436\u0435\u0442\u0435 \u0438 \u0440\u0430\u0441\u043f\u0440\u043e\u0441\u0442\u0440\u0430\u043d\u0438\u0442\u0435 \u0441\u043b\u043e\u0432\u043e \u043e \u0432\u0430\u0448\u0435\u043c \u043d\u043e\u0432\u043e\u043c \u0442\u0440\u0443\u0434\u0435. \u0422\u043e\u043b\u044c\u043a\u043e \u043d\u0435 \u0432\u044b\u0431\u0438\u0440\u0430\u0439\u0442\u0435 \u0443\u0436\u0435 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u043d\u044b\u0435 \u0434\u0430\u0442\u044b \u0434\u043b\u044f \u043d\u043e\u0432\u044b\u0445 \u0437\u0430\u043f\u0438\u0441\u0435\u0439, \u0435\u0441\u043b\u0438 \u0442\u043e\u043b\u044c\u043a\u043e \u043c\u044b \u043d\u0435 \u0434\u043e\u0441\u0442\u0438\u0433\u043b\u0438 \u043e\u0442\u043c\u0435\u0442\u043a\u0438 \u0432 366 \u0437\u0430\u043f\u0438\u0441\u0435\u0439...\n\n\u0423\u0434\u0430\u0447\u0438 \u0441 \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u0438\u0435\u043c \u0438 \u0441\u0442\u0443\u043f\u0430\u0439\u0442\u0435 \u0441\u043c\u0435\u043b\u0435\u0435, \u0421\u0442\u0440\u0430\u043d\u043d\u0438\u043a\u0438.\n\n[[/collapsible]]\n\n[[footnoteblock]] ", "tags": ["\u0431\u0438\u0431\u043b\u0438\u043e\u0442\u0435\u043a\u0430:\u0434\u043d\u0435\u0432\u043d\u0438\u043a_\u0438\u0434\u0443\u0449\u0435\u0433\u043e", "\u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0430:\u0445\u0430\u0431", "\u0444\u0438\u043b\u0438\u0430\u043b:en"], "parent": null, "locked": false}