u0435\u0442 \u0434\u043b\u044f \u043d\u0430\u0441 \u0441 \u0432\u0430\u043c\u0438 \u0432\u0437\u0430\u0438\u043c\u043d\u044b\u0439 \u0438\u043d\u0442\u0435\u0440\u0435\u0441. \u0412\u043d\u0443\u0442\u0440\u0438 \u043a\u043e\u043d\u0442\u0435\u0439\u043d\u0435\u0440\u0430 \u0438\u043c\u0435\u044e\u0442\u0441\u044f \u0442\u0430\u043a\u0436\u0435 \u0434\u0435\u0442\u0430\u043b\u0438. \u0411\u0443\u0434\u044c\u0442\u0435 \u0443\u0432\u0435\u0440\u0435\u043d\u044b, \u0447\u0442\u043e \u044d\u0442\u043e \u0431\u0443\u0434\u0435\u0442 \u043d\u0430\u0438\u043b\u0443\u0447\u0448\u0435\u0435 \u043c\u0435\u0441\u0442\u043e \u0434\u043b\u044f \u043d\u0435\u0433\u043e.//\n> \n> //\u0421\u0430\u043c\u043e \u0441\u043e\u0431\u043e\u0439 \u0440\u0430\u0437\u0443\u043c\u0435\u0435\u0442\u0441\u044f, \u0443 \u0432\u0430\u0441 \u0432\u043e\u0437\u043d\u0438\u043a\u043b\u043e \u043c\u043d\u043e\u0433\u043e \u0432\u043e\u043f\u0440\u043e\u0441\u043e\u0432. \u042f \u043d\u0435 \u043c\u043e\u0433\u0443 \u0440\u0430\u0437\u0433\u043b\u0430\u0448\u0430\u0442\u044c \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e\u0441\u0442\u0438, \u044d\u0442\u043e \u0431\u044b \u043f\u043e\u0448\u043b\u043e \u0432\u0440\u0430\u0437\u0440\u0435\u0437 \u0441 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u0430\u043c\u0438 \u043c\u043e\u0438\u0445 \u043a\u043e\u043b\u043b\u0435\u0433 \u0438 \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432. \u0422\u0435\u043c \u043d\u0435 \u043c\u0435\u043d\u0435\u0435, \u043f\u0435\u0440\u0435\u0434\u0430\u0447\u0430 \u0434\u0430\u043d\u043d\u043e\u0439 \u043f\u043e\u0441\u044b\u043b\u043a\u0438 \u0438\u043c\u0435\u0435\u0442 \u0441 \u043d\u0430\u0448\u0435\u0439 \u0441\u0442\u043e\u0440\u043e\u043d\u044b \u0432\u0435\u0441\u044c\u043c\u0430 \u0432\u0435\u0441\u043a\u0438\u0435 \u043f\u0440\u0438\u0447\u0438\u043d\u044b. \u041f\u0440\u043e\u0449\u0435 \u0433\u043e\u0432\u043e\u0440\u044f, \u043c\u044b \u043f\u0440\u043e\u0434\u0430\u0451\u043c \u0442\u043e\u043b\u044c\u043a\u043e \u043f\u0440\u0435\u0434\u043c\u0435\u0442\u044b \u0438\u0441\u043a\u0443\u0441\u0441\u0442\u0432\u0430 \u0438 \u0434\u0438\u043a\u043e\u0432\u0438\u043d\u043a\u0438. \u0418 \u0438\u043d\u043e\u0433\u0434\u0430, \u043e\u0447\u0435\u043d\u044c \u0440\u0435\u0434\u043a\u043e, \u043c\u044b \u0441\u0442\u0430\u043b\u043a\u0438\u0432\u0430\u0435\u043c\u0441\u044f \u0441 \u043f\u0440\u0435\u0434\u043c\u0435\u0442\u0430\u043c\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043d\u0435 \u043f\u043e\u0434\u0445\u043e\u0434\u044f\u0442 \u043f\u043e \u0441\u0432\u043e\u0435\u0439 \u043f\u0440\u0438\u0440\u043e\u0434\u0435 \u044d\u0442\u043e\u043c\u0443 \u0438\u043d\u0442\u0435\u0433\u0440\u0430\u043b\u044c\u043d\u043e\u043c\u0443 \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u0443, \u0430 \u0442\u043e \u0438 \u044f\u0432\u043d\u043e \u0435\u043c\u0443 \u0443\u0433\u0440\u043e\u0436\u0430\u044e\u0442.//\n> \n> //\u0422\u0430\u043a\u043e\u0432\u0430 \u043f\u0440\u0438\u0440\u043e\u0434\u0430 \u0438 \u044d\u0442\u043e\u0433\u043e \u043f\u0440\u0435\u0434\u043c\u0435\u0442\u0430. \u041d\u0430\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0431\u044b \u0446\u0435\u043d\u043d\u044b\u043c \u043e\u043d \u043d\u0438 \u0431\u044b\u043b, \u043c\u044b \u0432 \"\u041c\u0430\u0440\u0448\u0430\u043b\u043b, \u041a\u0430\u0440\u0442\u0435\u0440 \u0438 \u0414\u0430\u0440\u043a, \u041b\u0438\u043c\u0438\u0442\u0435\u0434\" \u0441\u0447\u0438\u0442\u0430\u0435\u043c, \u0447\u0442\u043e \u043a\u0430\u043a \u043d\u0430\u043c, \u0442\u0430\u043a \u0438 \u0424\u043e\u043d\u0434\u0443 SCP \u0431\u0443\u0434\u0435\u0442 \u0432\u0437\u0430\u0438\u043c\u043e\u0432\u044b\u0433\u043e\u0434\u043d\u043e, \u0447\u0442\u043e\u0431\u044b \u043e\u043d \u043e\u0441\u0442\u0430\u0432\u0430\u043b\u0441\u044f \u043d\u0430 \u0432\u0430\u0448\u0435\u043c \u043f\u043e\u043f\u0435\u0447\u0435\u043d\u0438\u0438. \u041e\u0431\u044b\u0447\u043d\u043e \u043d\u0430\u043c \u0434\u043e \u043e\u043f\u0440\u0435\u0434\u0435\u043b\u0451\u043d\u043d\u043e\u0439 \u0441\u0442\u0435\u043f\u0435\u043d\u0438 \u0431\u0435\u0437\u0440\u0430\u0437\u043b\u0438\u0447\u043d\u043e, \u0447\u0442\u043e \u043d\u0430\u0448\u0438 \u043a\u043b\u0438\u0435\u043d\u0442\u044b \u0434\u0435\u043b\u0430\u044e\u0442 \u0438\u043b\u0438 \u0447\u0435\u0433\u043e \u043d\u0435 \u0434\u0435\u043b\u0430\u044e\u0442 \u043f\u043e\u0441\u043b\u0435 \u0437\u0430\u0432\u0435\u0440\u0448\u0435\u043d\u0438\u044f \u0441\u0434\u0435\u043b\u043a\u0438. \u0422\u0435\u043c \u043d\u0435 \u043c\u0435\u043d\u0435\u0435, \u0432 \u0441\u0432\u0435\u0442\u0435 \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u0438\u0445 \u043e\u0442\u0434\u0435\u043b\u044c\u043d\u044b\u0445 \u0438\u043d\u0446\u0438\u0434\u0435\u043d\u0442\u043e\u0432, \u043a\u0430\u0441\u0430\u044e\u0449\u0438\u0445\u0441\u044f \u043d\u0430\u043c\u0435\u0440\u0435\u043d\u0438\u0439 \u043d\u0430\u0448\u0438\u0445 \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432 \u0432 \u043e\u0442\u043d\u043e\u0448\u0435\u043d\u0438\u0438 \u044d\u0442\u043e\u0433\u043e \u043a\u043e\u043d\u043a\u0440\u0435\u0442\u043d\u043e\u0433\u043e \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432\u0430, \u0438 \u0438\u0445 \u0443\u0431\u0435\u0436\u0434\u0435\u043d\u0438\u0439 \u0432 \u043e\u0431\u0449\u0435\u043c \u0438 \u0446\u0435\u043b\u043e\u043c, \u043c\u043e\u0436\u043d\u043e \u0441 \u0443\u0432\u0435\u0440\u0435\u043d\u043d\u043e\u0441\u0442\u044c\u044e \u043f\u0440\u0435\u0434\u043f\u043e\u043b\u043e\u0436\u0438\u0442\u044c, \u0447\u0442\u043e \u043f\u0440\u043e\u0434\u0430\u0436\u0430 \u0443\u043a\u0430\u0437\u0430\u043d\u043d\u043e\u0433\u043e \u043f\u0440\u0435\u0434\u043c\u0435\u0442\u0430 \u0431\u0443\u0434\u0435\u0442 \u043d\u0435 \u043b\u0443\u0447\u0448\u0438\u043c \u0434\u0435\u043b\u043e\u0432\u044b\u043c \u0440\u0435\u0448\u0435\u043d\u0438\u0435\u043c \u0441 \u043d\u0430\u0448\u0435\u0439 \u0441\u0442\u043e\u0440\u043e\u043d\u044b.//\n> \n> //\u0425\u0440\u0430\u043d\u0435\u043d\u0438\u0435 \u0442\u0430\u043a\u0436\u0435 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 \u0441\u043e\u0431\u043e\u0439 \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u0443, \u043f\u043e\u0441\u043a\u043e\u043b\u044c\u043a\u0443 \u043a\u0440\u0430\u0439\u043d\u044f\u044f \u0432\u043e\u0441\u0442\u0440\u0435\u0431\u043e\u0432\u0430\u043d\u043d\u043e\u0441\u0442\u044c \u0434\u0430\u043d\u043d\u043e\u0433\u043e \u0442\u043e\u0432\u0430\u0440\u0430 \u0441\u0442\u0430\u0432\u0438\u0442 \u043f\u043e\u0434 \u0443\u0433\u0440\u043e\u0437\u0443 \u0446\u0435\u043b\u0438 \u0438 \u0441\u0442\u0430\u0431\u0438\u043b\u044c\u043d\u044b\u0439 \u0431\u0438\u0437\u043d\u0435\u0441 \"\u041c\u0430\u0440\u0448\u0430\u043b\u043b, \u041a\u0430\u0440\u0442\u0435\u0440 \u0438 \u0414\u0430\u0440\u043a, \u041b\u0438\u043c\u0438\u0442\u0435\u0434\". \u041d\u0438\u043a\u043e\u043c\u0443 \u043d\u0435 \u043f\u043e\u043d\u0440\u0430\u0432\u0438\u0442\u0441\u044f \u0431\u044b\u0442\u044c \u043a\u0430\u0441\u0441\u0438\u0440\u043e\u043c \u0432 \u0431\u0430\u043d\u043a\u0435, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0432 \u043a\u0430\u0436\u0434\u044b\u0439 \u043c\u043e\u043c\u0435\u043d\u0442 \u0432\u0440\u0435\u043c\u0435\u043d\u0438 \u043d\u0430\u0445\u043e\u0434\u0438\u0442\u0441\u044f \u043f\u043e\u0434 \u0443\u0433\u0440\u043e\u0437\u043e\u0439 \u043e\u0433\u0440\u0430\u0431\u043b\u0435\u043d\u0438\u044f. \u041a\u043e\u0440\u043e\u0447\u0435 \u0433\u043e\u0432\u043e\u0440\u044f, \u0445\u0440\u0430\u043d\u0438\u0442\u044c \u044d\u0442\u043e\u0442 \u043f\u0440\u0435\u0434\u043c\u0435\u0442 \u043e\u0431\u0445\u043e\u0434\u0438\u0442\u0441\u044f \u0434\u043e\u0440\u043e\u0436\u0435, \u0447\u0435\u043c \u043e\u043d \u0441\u0430\u043c \u0441\u0442\u043e\u0438\u0442; \u044d\u0442\u043e \u0443\u0436 \u0441\u043b\u0438\u0448\u043a\u043e\u043c. \u0422\u0430\u043a \u0447\u0442\u043e \u0440\u0430\u0434\u0438 \u0437\u043d\u0430\u043c\u0435\u043d\u0438\u0442\u044b\u0445 \u0434\u043e\u0441\u0442\u043e\u043f\u0440\u0438\u043c\u0435\u0447\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u0435\u0439, \u043f\u0430\u043c\u044f\u0442\u043d\u0438\u043a\u043e\u0432, \u043d\u0435\u043a\u0438\u0445 \u043d\u0430\u0446\u0438\u0439, \u043d\u0430\u0448\u0435\u0439 \u0434\u0430\u043b\u044c\u043d\u0435\u0439\u0448\u0435\u0439 \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \u0438, \u0432\u043f\u043e\u043b\u043d\u0435 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e, \u0441\u043e\u043b\u043d\u0446\u0430 \u0438\u043b\u0438 \u043b\u0443\u043d\u044b, \u043c\u044b \u043f\u0435\u0440\u0435\u0434\u0430\u0451\u043c \u044d\u0442\u043e \u0442\u0435\u043c, \u043a\u0442\u043e \u0431\u043e\u043b\u0435\u0435 \u043e\u043f\u044b\u0442\u0435\u043d \u0438 \u0438\u0441\u043a\u0443\u0441\u0435\u043d \u0432 \u0441\u043e\u043a\u0440\u044b\u0442\u0438\u0438 \u0432\u0435\u0449\u0435\u0439 \u043e\u0442 \u043c\u0438\u0440\u0430.//\n> \n> //\u041f\u0440\u0438\u043c\u0438\u0442\u0435 \u0436\u0435, \u043f\u043e\u0436\u0430\u043b\u0443\u0439\u0441\u0442\u0430 \u0431\u0435\u0437\u0432\u043e\u0437\u043c\u0435\u0437\u0434\u043d\u044b\u0439 \u043f\u043e\u0434\u0430\u0440\u043e\u043a, \u043e\u0442 \u043d\u0430\u0441 \u2013 \u0432\u0430\u043c.//\n> \n> //P.S. \u041f\u0440\u0438\u043b\u0430\u0433\u0430\u0435\u043c \u0442\u0430\u043a\u0436\u0435 \u043a\u043e\u0440\u043e\u0431\u043a\u0443 \u043a\u043e\u043d\u0444\u0435\u0442 \"La Maison du Chocolat\" \u0438 \u0441\u0435\u0440\u0442\u0438\u0444\u0438\u043a\u0430\u0442 \u043d\u0430 \u0442\u043e\u0432\u0430\u0440\u043d\u044b\u0439 \u043a\u0440\u0435\u0434\u0438\u0442 \u0432 \u0440\u0430\u0437\u043c\u0435\u0440\u0435 \u2588\u2588\u2588\u2588 \u0444\u0443\u043d\u0442\u043e\u0432 \u0441\u0442\u0435\u0440\u043b\u0438\u043d\u0433\u043e\u0432, \u0447\u0442\u043e\u0431\u044b \u043f\u043e\u0434\u0447\u0435\u0440\u043a\u043d\u0443\u0442\u044c \u043d\u0430\u0448\u0443 \u0432\u0430\u043c \u0431\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u043d\u043e\u0441\u0442\u044c.//\n> \n> //\u0411\u0435\u0441\u043a\u043e\u043d\u0435\u0447\u043d\u043e \u043f\u0440\u0438\u0437\u043d\u0430\u0442\u0435\u043b\u044c\u043d\u0430,//\n> \n> [[image https://scpfoundation.net/local--files/scp-2501/LEAC1.png width=\"350px\"]]\n> +++++ //\u041b\u0435\u0434\u0438 \u042d\u043c\u0438\u043b\u0438 \u041e\u043b\u043a\u043e\u0442\u0442 \u041a\u0430\u0440\u0442\u0435\u0440//\n> ++++++ //8^^\u0435^^ \u0438\u044e\u043d\u044f 1968//\n[[/collapsible]]\n\n\u041f\u0440\u0435\u0434\u043c\u0435\u0442\u044b, \u0432\u043a\u043b\u044e\u0447\u0451\u043d\u043d\u044b\u0435 \u0432 \u043f\u043e\u0441\u044b\u043b\u043a\u0443, \u043a\u0440\u043e\u043c\u0435 \u0441\u0430\u043c\u043e\u0433\u043e SCP-2501, \u0431\u044b\u043b\u0438 \u0437\u0430\u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u044b \u0438/\u0438\u043b\u0438 \u0443\u043d\u0438\u0447\u0442\u043e\u0436\u0435\u043d\u044b. \u041e\u0444\u0438\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u043e\u0442\u0432\u0435\u0442\u0430 \u043e\u0442 \u0424\u043e\u043d\u0434\u0430 \u043d\u0435 \u043f\u0440\u0435\u0434\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u043b\u043e\u0441\u044c.\n\n----\n\n\n= [[size 90%]]**<< [[[SCP-2500]]] | SCP-2501 | [[[SCP-2502]]] >>**[[/size]]", "tags": ["\u043a\u043b\u0430\u0441\u0441:\u0431\u0435\u0437\u043e\u043f\u0430\u0441\u043d\u044b\u0439", "\u0441\u0432\u043e\u0439\u0441\u0442\u0432\u043e:\u043c\u0435\u0445\u0430\u043d\u0438\u0437\u043c", "\u0441\u0432\u043e\u0439\u0441\u0442\u0432\u043e:\u043f\u0440\u0435\u0434\u043c\u0435\u0442", "\u0441\u043e_\u043b\u043f\u0438:\u043c\u043a\u0438\u0434", "\u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0430:\u043e\u0431\u044a\u0435\u043a\u0442", "\u0444\u0438\u043b\u0438\u0430\u043b:en"], "parent": null, "locked": false}