43e\u0433\u0434\u0430 \u0446\u0438\u0440\u043a \u043f\u0440\u0438\u0435\u0437\u0436\u0430\u0435\u0442 \u0432 \u0433\u043e\u0440\u043e\u0434, \u0438 \u043d\u0438\u043a\u0442\u043e \u043d\u0435 \u043c\u043e\u0436\u0435\u0442 \u043f\u043e\u043d\u044f\u0442\u044c, \u043e\u0442\u043a\u0443\u0434\u0430 \u043e\u043d \u043f\u0440\u0438\u0435\u0445\u0430\u043b, \u0438 \u043a\u0430\u043a \u0443\u0445\u0438\u0442\u0440\u0438\u043b\u0441\u044f \u043d\u0435\u0437\u0430\u043c\u0435\u0442\u043d\u043e \u0440\u0430\u0437\u0431\u0438\u0442\u044c \u043b\u0430\u0433\u0435\u0440\u044c. \u0427\u0438\u043d\u043e\u0432\u043d\u0438\u043a\u0438 \u0431\u044b\u0432\u0430\u044e\u0442 \u043e\u0437\u0430\u0434\u0430\u0447\u0435\u043d\u044b \u0432\u043d\u0435\u0437\u0430\u043f\u043d\u044b\u043c \u043f\u043e\u044f\u0432\u043b\u0435\u043d\u0438\u0435\u043c \u0448\u0430\u0442\u0440\u043e\u0432 \u0432 \u043e\u043a\u0440\u0443\u0436\u0435\u043d\u0438\u0438 \u043a\u0430\u0440\u0430\u0432\u0430\u043d\u0430 \u043f\u0440\u0438\u0446\u0435\u043f\u043e\u0432 \u0438 \u0444\u0443\u0440\u0433\u043e\u043d\u043e\u0432, \u043d\u043e, \u043a\u0430\u043a \u043d\u0438 \u0441\u0442\u0440\u0430\u043d\u043d\u043e, \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u044b \u0432\u0441\u0435\u0433\u0434\u0430 \u0432 \u043f\u043e\u0440\u044f\u0434\u043a\u0435.\n\n\u0418, \u043a\u043e\u043d\u0435\u0447\u043d\u043e, \u043f\u043e\u0441\u0435\u0442\u0438\u0432 \u0448\u043e\u0443, \u043e\u043d\u0438 \u0442\u0435\u0440\u044f\u044e\u0442 \u043b\u044e\u0431\u044b\u0435 \u0441\u043e\u043c\u043d\u0435\u043d\u0438\u044f, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u043c\u043e\u0433\u043b\u0438 \u0443 \u043d\u0438\u0445 \u0431\u044b\u0442\u044c. \u041f\u043e\u0442\u043e\u043c\u0443 \u0447\u0442\u043e, \u0432 \u043a\u043e\u043d\u0446\u0435 \u043a\u043e\u043d\u0446\u043e\u0432, \u044d\u0442\u043e \u0438 **\u0435\u0441\u0442\u044c** \u0432\u0435\u043b\u0438\u0447\u0430\u0439\u0448\u0435\u0435 \u0448\u043e\u0443 \u043d\u0430 \u0437\u0435\u043c\u043b\u0435.\n\n\u0426\u0438\u0440\u043a \u043e\u0442\u043d\u043e\u0441\u0438\u0442\u0441\u044f \u043a \u0441\u0432\u043e\u0438\u043c \u0438\u0441\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044f\u043c, \u043a\u0430\u043a \u043a \u0446\u0435\u043d\u043d\u044b\u043c \u0430\u043a\u0442\u0438\u0432\u0430\u043c, \u0431\u0443\u0434\u044c \u0442\u043e \u044d\u043a\u0432\u0438\u043b\u0438\u0431\u0440\u0438\u0441\u0442\u044b, \u0441\u043e\u0441\u0442\u043e\u044f\u0449\u0438\u0435 \u0438\u0437 \u043e\u0434\u043d\u0438\u0445 \u0442\u043e\u043b\u044c\u043a\u043e \u043d\u043e\u0433, \u043d\u0435\u0447\u0442\u043e \u0441\u043e \u0449\u0443\u043f\u0430\u043b\u044c\u0446\u0430\u043c\u0438, \u0440\u0430\u0437\u0433\u043e\u0432\u0430\u0440\u0438\u0432\u0430\u044e\u0449\u0435\u0435 \u043d\u0435\u0432\u043d\u044f\u0442\u043d\u043e \u0438 \u0432\u0438\u0437\u0433\u043b\u0438\u0432\u043e, \u0438\u043b\u0438 \u043a\u043b\u043e\u0443\u043d\u044b \u0441\u043e \u0441\u043b\u0435\u0437\u0430\u043c\u0438 \u0442\u044b\u0441\u044f\u0447\u0438 \u043c\u0451\u0440\u0442\u0432\u044b\u0445 \u0437\u0432\u0435\u0437\u0434 \u0432 \u0433\u043b\u0430\u0437\u0430\u0445. \u0426\u0438\u0440\u043a -- \u0441\u0435\u043c\u044c\u044f, \u0438 \u0441\u0435\u043c\u044c\u044f \u043e\u0441\u0442\u0430\u0451\u0442\u0441\u044f \u0432\u043c\u0435\u0441\u0442\u0435.\n\n\u0422\u0430\u043a \u043f\u0440\u0438\u0445\u043e\u0434\u0438\u0442\u0435 \u0436\u0435 \u0441\u043c\u043e\u0442\u0440\u0435\u0442\u044c \u0448\u043e\u0443, \u043f\u0440\u0438\u0445\u043e\u0434\u0438\u0442\u0435 \u043f\u0440\u0438\u043a\u043e\u0441\u043d\u0443\u0442\u044c\u0441\u044f \u043a \u0432\u043e\u043b\u0448\u0435\u0431\u0441\u0442\u0432\u0443! \u0412\u0435\u0434\u044c, \u0432 \u043a\u043e\u043d\u0446\u0435 \u043a\u043e\u043d\u0446\u043e\u0432, \u0440\u0430\u0437\u0432\u0435 \u0432\u044b \u0447\u0442\u043e-\u0442\u043e \u043f\u043e\u0442\u0435\u0440\u044f\u0435\u0442\u0435?\n[[/=]]\n[[/div]]\n[[/tab]]\n\n