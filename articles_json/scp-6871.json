;\n}\n[[/module]]\n\n[[module CSS]]\n\u00a0\u00a0\u00a0body {\n\u00a0\u00a0\u00a0\u00a0--barColour: #21284E\n}\n[[/module]]\n\n\n-----\n**\u041e\u0441\u043e\u0431\u044b\u0435 \u0443\u0441\u043b\u043e\u0432\u0438\u044f \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u044f:** \u0418\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0438\u0438 \u043f\u043e \u0432\u044b\u043f\u043e\u043b\u043d\u0435\u043d\u0438\u044e SCP-6871 \u0438 \u0441\u043f\u0438\u0441\u043e\u043a \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u044b\u0445 \u0434\u043b\u044f \u044d\u0442\u043e\u0433\u043e \u0438\u043d\u0433\u0440\u0435\u0434\u0438\u0435\u043d\u0442\u043e\u0432 \u0434\u043e\u043b\u0436\u043d\u044b \u0431\u044b\u0442\u044c \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u044b \u0438\u0441\u043a\u043b\u044e\u0447\u0438\u0442\u0435\u043b\u044c\u043d\u043e --\u0421\u043e\u0432\u0435\u0442\u0443 O5-- \u0410\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430\u0442\u043e\u0440\u0443. \u0412\u0441\u0435\u0445 \u043f\u043e\u0441\u0442\u043e\u0440\u043e\u043d\u043d\u0438\u0445 \u043b\u0438\u0446, \u043f\u044b\u0442\u0430\u044e\u0449\u0438\u0445\u0441\u044f \u043f\u043e\u043b\u0443\u0447\u0438\u0442\u044c \u0434\u043e\u0441\u0442\u0443\u043f \u043a \u0434\u0430\u043d\u043d\u043e\u0439 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0438, \u0441\u043b\u0435\u0434\u0443\u0435\u0442 \u0443\u0441\u0442\u0440\u0430\u043d\u044f\u0442\u044c \u043d\u0430 \u043c\u0435\u0441\u0442\u0435. \n\n\u0412\u0441\u0435 \u044d\u043a\u0441\u043f\u0435\u0440\u0438\u043c\u0435\u043d\u0442\u044b \u0441 SCP-6871 \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e \u043f\u0440\u043e\u0432\u043e\u0434\u0438\u0442\u044c \u0432 \u043f\u0440\u0438\u0441\u0443\u0442\u0441\u0442\u0432\u0438\u0438 \u043a\u0430\u043a \u043c\u0438\u043d\u0438\u043c\u0443\u043c 5 \u0441\u0432\u0438\u0434\u0435\u0442\u0435\u043b\u0435\u0439[[footnote]] \u0412 \u043d\u0430\u0441\u0442\u043e\u044f\u0449\u0435\u0435 \u0432\u0440\u0435\u043c\u044f \u0432 \u0441\u043e\u0441\u0442\u0430\u0432 \u0433\u0440\u0443\u043f\u043f\u044b \u0441\u0432\u0438\u0434\u0435\u0442\u0435\u043b\u0435\u0439 \u0432\u0445\u043e\u0434\u044f\u0442 \u0410\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430\u0442\u043e\u0440, \u041c\u0430\u0440\u0438\u044f \u0414\u0436\u043e\u043d\u0441, \u041e5-1, \u041e5-13 \u0438 \u0434-\u0440 \u0413\u0438\u0440\u0441. [[/footnote]], \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0434\u043e\u043b\u0436\u043d\u044b \u0432 \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u043c \u043f\u043e\u0440\u044f\u0434\u043a\u0435 \u043d\u0430\u0445\u043e\u0434\u0438\u0442\u044c\u0441\u044f \u0432 [[[SCP-3936|\u0418\u0437\u043e\u043b\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u043e\u0439 \u0417\u043e\u043d\u0435 01]]] \u043d\u0430\u0440\u044f\u0434\u0443 \u0441 \u0444\u0430\u0439\u043b\u043e\u043c SCP-6871 \u043d\u0430 \u043f\u0440\u043e\u0442\u044f\u0436\u0435\u043d\u0438\u0438 \u0432\u0441\u0435\u0439 \u043f\u0440\u043e\u0446\u0435\u0434\u0443\u0440\u044b. \u0422\u0430\u043a\u0436\u0435 \u0432\u043e \u0438\u0437\u0431\u0435\u0436\u0430\u043d\u0438\u0435 \u0432\u043e\u0437\u043d\u0438\u043a\u043d\u043e\u0432\u0435\u043d\u0438\u044f \u043f\u0430\u0440\u0430\u0434\u043e\u043a\u0441\u0430\u043b\u044c\u043d\u043e\u0439 \u0432\u0440\u0435\u043c\u0435\u043d\u043d\u043e\u0439 \u043f\u0435\u0442\u043b\u0438 \u0447\u0435\u043b\u043e\u0432\u0435\u043a, \u043f\u0440\u043e\u0432\u043e\u0434\u044f\u0449\u0438\u0439 \u044d\u043a\u0441\u043f\u0435\u0440\u0438\u043c\u0435\u043d\u0442, \u0434\u043e\u043b\u0436\u0435\u043d \u043d\u0430\u0445\u043e\u0434\u0438\u0442\u044c\u0441\u044f \u043d\u0430 \u0442\u0435\u0440\u0440\u0438\u0442\u043e\u0440\u0438\u0438 \u0443\u043a\u0430\u0437\u0430\u043d\u043d\u043e\u0439 \u0417\u043e\u043d\u044b, \u0430 \u043e\u0431\u044a\u0435\u043a\u0442 \u0440\u0438\u0442\u0443\u0430\u043b\u0430 \u2013 \u0437\u0430 \u0435\u0451 \u043f\u0440\u0435\u0434\u0435\u043b\u0430\u043c\u0438.\n\n**\u041e\u043f\u0438\u0441\u0430\u043d\u0438\u0435:** SCP-6871 \u2013 \u044d\u0442\u043e \u0442\u0430\u0443\u043c\u0430\u0442\u0443\u0440\u0433\u0438\u0447\u0435\u0441\u043a\u0438\u0439 \u0440\u0438\u0442\u0443\u0430\u043b, \u043f\u043e\u0437\u0432\u043e\u043b\u044f\u044e\u0449\u0438\u0439 \u0443\u0434\u0430\u043b\u044f\u0442\u044c \u043b\u044e\u0434\u0435\u0439, \u043e\u0431\u044a\u0435\u043a\u0442\u044b \u0438/\u0438\u043b\u0438 \u044f\u0432\u043b\u0435\u043d\u0438\u044f \u0438\u0437 \u043e\u0441\u043d\u043e\u0432\u043d\u043e\u0439 \u0432\u0440\u0435\u043c\u0435\u043d\u043d\u043e\u0439 \u043b\u0438\u043d\u0438\u0438. \u041f\u0440\u0438 \u043d\u0430\u0434\u043b\u0435\u0436\u0430\u0449\u0435\u043c \u0432\u044b\u043f\u043e\u043b\u043d\u0435\u043d\u0438\u0438 \u0440\u0438\u0442\u0443\u0430\u043b\u0430 \u0432\u043b\u0438\u044f\u043d\u0438\u0435 \u0438 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u044f, \u043e\u0441\u0443\u0449\u0435\u0441\u0442\u0432\u043b\u0451\u043d\u043d\u044b\u0435 \u043e\u0431\u044a\u0435\u043a\u0442\u043e\u043c \u0440\u0438\u0442\u0443\u0430\u043b\u0430 \u0432 \u043f\u0440\u0435\u0434\u0435\u043b\u0430\u0445 \u0432\u0440\u0435\u043c\u0435\u043d\u043d\u043e\u0439 \u043b\u0438\u043d\u0438\u0438, \u0431\u0443\u0434\u0443\u0442 \u0440\u0435\u0442\u0440\u043e\u0430\u043a\u0442\u0438\u0432\u043d\u043e \u0443\u0434\u0430\u043b\u0435\u043d\u044b \u0438 \u0437\u0430\u043c\u0435\u043d\u0435\u043d\u044b \u0434\u0440\u0443\u0433\u0438\u043c\u0438.\n\n**\u041e\u0431\u043d\u0430\u0440\u0443\u0436\u0435\u043d\u0438\u0435:** [[[[SCP-6936| \u0423\u0434\u0430\u043b\u0435\u043d\u043e \u043f\u043e \u044d\u043a\u0441\u0442\u0440\u0435\u043d\u043d\u043e\u043c\u0443 \u0440\u0430\u0441\u043f\u043e\u0440\u044f\u0436\u0435\u043d\u0438\u044e \u0434\u0438\u0440\u0435\u043a\u0442\u043e\u0440\u0430 \u0410\u041f\u0410\u0418\u0411 \u041c\u0430\u0440\u0438\u0438 \u0414\u0436\u043e\u043d\u0441]]]]\n\n**\u041f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0435 6871-1:** \u041f\u0440\u043e\u0442\u043e\u043a\u043e\u043b \u044d\u043a\u0441\u043f\u0435\u0440\u0438\u043c\u0435\u043d\u0442\u043e\u0432 \u0441 SCP-6871\n\n||~ \u041d\u043e\u043c\u0435\u0440 \u044d\u043a\u0441\u043f\u0435\u0440\u0438\u043c\u0435\u043d\u0442\u0430 ||~ \u041b\u0438\u0446\u043e, \u043f\u0440\u043e\u0432\u043e\u0434\u044f\u0449\u0435\u0435 \u044d\u043a\u0441\u043f\u0435\u0440\u0438\u043c\u0435\u043d\u0442 ||~ \u041e\u0431\u044a\u0435\u043a\u0442 \u0440\u0438\u0442\u0443\u0430\u043b\u0430 ||~ \u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442 ||\n|| \u21161 || D-51832 || \u041e\u0434\u043d\u043e\u0434\u043e\u043b\u043b\u0430\u0440\u043e\u0432\u0430\u044f \u043a\u0443\u043f\u044e\u0440\u0430, \u0432\u044b\u0431\u0440\u0430\u043d\u043d\u0430\u044f \u0441\u043b\u0443\u0447\u0430\u0439\u043d\u044b\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c. || \u041d\u0438\u043a\u0430\u043a\u0438\u0445 \u0438\u0437\u043c\u0435\u043d\u0435\u043d\u0438\u0439 \u043d\u0435 \u0432\u044b\u044f\u0432\u043b\u0435\u043d\u043e. ||\n|| \u21162 || D-51832 || \u041e\u0434\u043d\u043e\u0434\u043e\u043b\u043b\u0430\u0440\u043e\u0432\u0430\u044f \u043a\u0443\u043f\u044e\u0440\u0430, \u0440\u0430\u043d\u0435\u0435 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u043d\u0430\u044f \u0434-\u0440\u043e\u043c \u0411\u0440\u0435\u043d\u0434\u043e\u0439 \u043f\u0440\u0438 \u043f\u043e\u043a\u0443\u043f\u043a\u0435 \u043a\u043e\u0444\u0435. || \u0414-\u0440 \u0411\u0440\u0435\u043d\u0434\u0430 \u0431\u044b\u043b\u0430 \u0437\u0430\u043c\u0435\u0442\u043d\u043e \u0431\u043e\u043b\u0435\u0435 \u0443\u0441\u0442\u0430\u0432\u0448\u0435\u0439 \u043d\u0430 \u043f\u0440\u043e\u0442\u044f\u0436\u0435\u043d\u0438\u0438 \u0432\u0441\u0435\u0433\u043e \u0434\u043d\u044f, \u0432 \u0441\u0432\u044f\u0437\u0438 \u0441 \u0447\u0435\u043c \u0442\u0435\u043c\u043f \u043e\u0441\u0443\u0449\u0435\u0441\u0442\u0432\u043b\u0435\u043d\u0438\u044f \u043f\u0440\u043e\u0435\u043a\u0442\u0430, \