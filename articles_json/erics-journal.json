e \u0441\u0442\u043e\u0440\u043e\u043d\u0443 \u043a\u043b\u0435\u0435\u043c \u0441 \u0431\u043b\u0451\u0441\u0442\u043a\u0430\u043c\u0438\n@@ @@\n\u041f\u043e\u043b\u043e\u0436\u0438\u0442\u044c \u0432 \u043c\u0438\u043a\u0440\u043e\u0432\u043e\u043b\u043d\u043e\u0432\u043a\u0443, \u0434\u0432\u0430\u0436\u0434\u044b \u043d\u0430\u0436\u0430\u0442\u044c \u043d\u0430 \u043a\u043d\u043e\u043f\u043a\u0443 \"5\" (\u0431\u044b\u0442\u044c \u043e\u0441\u0442\u043e\u0440\u043e\u0436\u043d\u0435\u0435, \u043d\u0435 \u043d\u0430\u0436\u0430\u0442\u044c \u043d\u0430 \"4\")\n@@ @@\n\u041e\u0441\u0442\u0443\u0434\u0438\u0442\u044c \u0441 \u043f\u043e\u043c\u043e\u0449\u044c\u044e \u043b\u044c\u0434\u0430 \u0438\u0437 \u0445\u043e\u043b\u043e\u0434\u0438\u043b\u044c\u043d\u0438\u043a\u0430\n@@ @@\n\u042f \u0441\u0434\u0435\u043b\u0430\u043b \u0443\u0436\u0435 \u0434\u0432\u0430, \u043d\u043e \u043c\u0430\u043c\u0430 \u0440\u0430\u0437\u043e\u0437\u043b\u0438\u043b\u0430\u0441\u044c \u0438\u0437-\u0437\u0430 \u0442\u043e\u0433\u043e, \u0447\u0442\u043e \u0443 \u043c\u0435\u043d\u044f \u043a\u043e\u043d\u0447\u0438\u043b\u0438\u0441\u044c \u0432\u0435\u043b\u043e\u0441\u0438\u043f\u0435\u0434\u043d\u044b\u0435 \u0448\u043b\u0435\u043c\u044b. \u0413\u0434\u0435 \u043c\u043d\u0435 \u0434\u043e\u0441\u0442\u0430\u0442\u044c \u0435\u0449\u0451?\n@@ @@\n@@ @@\n@@ @@\n@@ @@\n@@ @@\n@@ @@\n@@ @@\n[[/div]]\n\n[[div class=\"page\"]]\n\u0414\u043e\u0440\u043e\u0433\u043e\u0439 \u0434\u043d\u0435\u0432\u043d\u0438\u043a, \u0432\u0447\u0435\u0440\u0430 \u043a\u0430\u043a\u043e\u0439-\u0442\u043e \u0441\u0442\u0440\u0430\u043d\u043d\u044b\u0439 \u043f\u0430\u0440\u0435\u043d\u044c \u0438\u0437 \u0438\u043d\u0442\u0435\u0440\u043d\u0435\u0442\u0430 \u0433\u043e\u0432\u043e\u0440\u0438\u043b \u0441\u043e \u043c\u043d\u043e\u0439 \u043d\u0430\u0441\u0447\u0451\u0442 \u0448\u043b\u0435\u043c\u043e\u0432\n@@ @@\n\u041e\u043d \u0433\u043e\u0432\u043e\u0440\u0438\u0442, \u0447\u0442\u043e \u0445\u043e\u0447\u0435\u0442 \u0441\u0434\u0435\u043b\u0430\u0442\u044c \u0431\u043e\u043b\u044c\u0448\u0435. \u042f \u043d\u0435 \u043f\u043e\u043d\u0438\u043c\u0430\u044e, \u0437\u0430\u0447\u0435\u043c \u0435\u043c\u0443 \u0431\u043e\u043b\u044c\u0448\u0435, \u044f \u0441\u0434\u0435\u043b\u0430\u043b \u0442\u043e\u043b\u044c\u043a\u043e \u0434\u0432\u0430\n@@ @@\n\u042f \u0441 \u0434\u0440\u0443\u0437\u044c\u044f\u043c\u0438 \u043f\u043e\u0434\u0443\u043c\u0430\u043b, \u0447\u0442\u043e \u0445\u043e\u0440\u043e\u0448\u043e \u0431\u044b \u0437\u0430\u0432\u0435\u0441\u0442\u0438 \u0435\u0449\u0451 \u0434\u0440\u0443\u0437\u0435\u0439, \u0442\u0430\u043a \u0447\u0442\u043e \u044f \u043f\u043e\u043c\u043e\u0433 \u0435\u043c\u0443,\n@@ @@\n\u041d\u043e \u044f \u043d\u0435 \u0437\u043d\u0430\u044e, \u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u043e \u043b\u0438 \u044f \u043f\u043e\u0441\u0442\u0443\u043f\u0438\u043b, \u0434\u043d\u0435\u0432\u043d\u0438\u043a.\n@@ @@\n\u0427\u0430\u0440\u043b\u044c\u0437 \u0441\u043a\u0430\u0437\u0430\u043b, \u0447\u0442\u043e \u0442\u044b \u043f\u043e\u0441\u0442\u0443\u043f\u0430\u0435\u0448\u044c \u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u043e, \u0435\u0441\u043b\u0438 \u043f\u043e\u043c\u043e\u0433\u0430\u0435\u0448\u044c \u043b\u044e\u0434\u044f\u043c\n@@ @@\n\u041e\u043d \u0442\u0430\u043a\u0436\u0435 \u0445\u043e\u0442\u0435\u043b \u043b\u0443\u0447\u0435\u0432\u0443\u044e \u043f\u0443\u0448\u043a\u0443. \u041c\u043d\u0435 \u043a\u0430\u0437\u0430\u043b\u043e\u0441\u044c, \u0447\u0442\u043e \u044f \u043d\u0438\u043a\u043e\u043c\u0443 \u043f\u0440\u043e \u043d\u0435\u0451 \u043d\u0435 \u0433\u043e\u0432\u043e\u0440\u0438\u043b\n@@ @@\n\u042f \u043f\u043e\u043f\u0440\u043e\u0441\u0438\u043b \u042d\u0441\u043a\u0438\u043c\u043e, \u0447\u0442\u043e\u0431\u044b \u043e\u043d \u0432\u0435\u0440\u043d\u0443\u043b \u043c\u043d\u0435 \u043b\u0443\u0447\u0435\u0432\u0443\u044e \u043f\u0443\u0448\u043a\u0443\n@@ @@\n\u041b\u0443\u0447\u0435\u0432\u0430\u044f \u043f\u0443\u0448\u043a\u0430 \u043e\u0447\u0435\u043d\u044c \u0432\u0430\u0436\u043d\u0430 \u0434\u043b\u044f \u043c\u0435\u043d\u044f, \u0438 \u044f \u0434\u0430\u043b \u0435\u043c\u0443 \u0435\u0451, \u0447\u0442\u043e\u0431\u044b \u043f\u043e\u043c\u043e\u0447\u044c. \u041d\u0430\u0434\u0435\u044e\u0441\u044c, \u044f \u043f\u043e\u043b\u0443\u0447\u0443 \u0435\u0451 \u043e\u0431\u0440\u0430\u0442\u043d\u043e\n@@ @@\n\u042f \u0437\u043d\u0430\u044e, \u0447\u0442\u043e \u0434\u043e\u0432\u0435\u0440\u044f\u0442\u044c \u043c\u043e\u0436\u043d\u043e \u043d\u0435 \u0432\u0441\u0435\u043c, \u043d\u043e \u043e\u043d \u0445\u043e\u0442\u0435\u043b \u0437\u0430\u0432\u0435\u0441\u0442\u0438 \u0434\u0440\u0443\u0437\u0435\u0439,\n@@ @@\n\u043e\u043d \u043c\u043e\u0433 \u043f\u0440\u043e\u0441\u0442\u043e \u043f\u043e\u043f\u0440\u043e\u0441\u0438\u0442\u044c, \u044f \u0431\u044b \u0441\u0442\u0430\u043b \u0435\u0433\u043e \u0434\u0440\u0443\u0433\u043e\u043c\n@@ @@\n\u0441\u043f\u043e\u043a\u043e\u0439\u043d\u043e\u0439 \u043d\u043e\u0447\u0438, \u0434\u043d\u0435\u0432\u043d\u0438\u043a.\n[[/div]]\n\n[[div class=\"page\"]]\n\u0414\u043e\u0440\u043e\u0433\u043e\u0439 \u0434\u043d\u0435\u0432\u043d\u0438\u043a, \u042d\u0441\u043a\u0438\u043c\u043e \u0441\u043a\u0430\u0437\u0430\u043b, \u0447\u0442\u043e \u044f \u043d\u0435 \u0434\u043e\u043b\u0436\u0435\u043d \u0431\u044b\u043b \u0434\u0430\u0432\u0430\u0442\u044c \u0435\u043c\u0443 \u043b\u0443\u0447\u0435\u0432\u0443\u044e \u043f\u0443\u0448\u043a\u0443,\n@@ @@\n\u042f \u0441\u043a\u0430\u0437\u0430\u043b \u042d\u0441\u043a\u0438\u043c\u043e, \u0447\u0442\u043e \u043e\u043d \u043d\u0435 \u043f\u0440\u0430\u0432, \u043f\u043e\u0442\u043e\u043c\u0443 \u0447\u0442\u043e \u043e\u043d \u043f\u0440\u043e\u0441\u0442\u043e \u0447\u0443\u0447\u0435\u043b\u043e \u0438 \u043d\u0438\u0447\u0435\u0433\u043e \u043d\u0435 \u0437\u043d\u0430\u0435\u0442 \u043e \u043d\u0430\u0441\u0442\u043e\u044f\u0449\u0438\u0445 \u043b\u044e\u0434\u044f\u0445\n@@ @@\n\u0417\u0430\u0442\u0435\u043c \u043e\u043d \u0437\u0430\u043f\u043b\u0430\u043a\u0430\u043b \u0438 \u0442\u0435\u043f\u0435\u0440\u044c \u043d\u0435 \u0440\u0430\u0437\u0433\u043e\u0432\u0430\u0440\u0438\u0432\u0430\u0435\u0442 \u0441\u043e \u043c\u043d\u043e\u0439.\n@@ @@\n@@ @@\n\u0418\u043d\u043e\u0433\u0434\u0430 \u043c\u043d\u0435 \u0445\u043e\u0447\u0435\u0442\u0441\u044f, \u0447\u0442\u043e\u0431\u044b \u0443 \u043c\u0435\u043d\u044f \u0431\u044b\u043b\u043e \u0431\u043e\u043b\u044c\u0448\u0435 \u0434\u0440\u0443\u0437\u0435\u0439, \u043d\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u0447\u0443\u0447\u0435\u043b\u0430 \u0438 \u043c\u0430\u043c\u0430,\n@@ @@\n\u043f\u043e\u0442\u043e\u043c\u0443 \u0447\u0442\u043e \u0447\u0443\u0447\u0435\u043b\u0430 \u0441\u043a\u0443\u0447\u043d\u044b\u0435, \u0430 \u043c\u0430\u043c\u0430 \u0432\u0441\u0435\u0433\u0434\u0430 \u043d\u0430 \u0440\u0430\u0431\u043e\u0442\u0435.\n@@ @@\n@@ @@\n@@ @@\n@@ @@\n@@ @@\n@@ @@\n@@ @@\n@@ @@\n@@ @@\n@@ @@\n[[/div]]\n\n[[div class=\"page\"]]\n\u041e\u0448\u0435\u0439\u043d\u0438\u043a, \u043f\u043e\u0437\u0432\u043e\u043b\u044f\u044e\u0449\u0438\u0439 \u0436\u0438\u0432\u043e\u0442\u043d\u044b\u043c \u0440\u0430\u0437\u0433\u043e\u0432\u0430\u0440\u0438\u0432\u0430\u0442\u044c\n@@ @@\n@@ @@\n\u0414\u043e\u0441\u0442\u0430\u0442\u044c \u043e\u0448\u0435\u0439\u043d\u0438\u043a \u0414\u0436\u0435\u043b\u043b\u0438\n@@ @@\n\u0414\u043e\u0441\u0442\u0430\u0442\u044c \u0434\u0438\u043d\u0430\u043c\u0438\u043a \u0438\u0437 \u0441\u0442\u0430\u0440\u043e\u0433\u043e \u043f\u043b\u044e\u0448\u0435\u0432\u043e\u0433\u043e \u043c\u0435\u0434\u0432\u0435\u0434\u044f, \u043e\u043d \u043d\u0435 \u0431\u0443\u0434\u0435\u0442 \u043f\u0440\u043e\u0442\u0438\u0432, \u044d\u0442\u043e \u0434\u043b\u044f \u0431\u043b\u0430\u0433\u043e\u0439 \u0446\u0435\u043b\u0438\n@@ @@\n\u041f\u043e\u0441\u043c\u043e\u0442\u0440\u0435\u0442\u044c \u0432\u0438\u0434\u0435\u043e