38 \u0444\u0443\u043d\u043a\u0446\u0438\u044f SCP-963 \u2014 \u043e\u0441\u0442\u0430\u0435\u0442\u0441\u044f \u043d\u0435\u0438\u0437\u0432\u0435\u0441\u0442\u043d\u044b\u043c. \u041f\u0440\u043e\u0442\u0435\u0441\u0442\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u044d\u0442\u043e \u043d\u0435\u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e.\n\n\u0414\u0436\u0435\u043a \u0434\u0435\u0439\u0441\u0442\u0432\u0443\u0435\u0442 \u043a\u0430\u043a \u043d\u0435\u043e\u0444\u0438\u0446\u0438\u0430\u043b\u044c\u043d\u044b\u0439 \u0431\u0443\u043a\u043c\u0435\u043a\u0435\u0440 \u0424\u043e\u043d\u0434\u0430. \u0415\u0441\u043b\u0438 \u043a\u0442\u043e-\u0442\u043e \u043d\u0430 \u0447\u0442\u043e-\u0442\u043e \u0441\u0442\u0430\u0432\u0438\u0442, \u0432\u0435\u043b\u0438\u043a\u0430 \u0432\u0435\u0440\u043e\u044f\u0442\u043d\u043e\u0441\u0442\u044c, \u0447\u0442\u043e \u0441\u0442\u0430\u0432\u043a\u0438 \u0434\u0435\u0440\u0436\u0438\u0442 \u0414\u043e\u043a. \u041c\u044b \u043c\u043e\u0433\u043b\u0438 \u0431\u044b \u0435\u0433\u043e \u0443\u043d\u044f\u0442\u044c, \u043d\u043e \u0442\u043e\u0442\u0430\u043b\u0438\u0437\u0430\u0442\u043e\u0440 \u0441\u043b\u0443\u0436\u0438\u0442 \u043a\u0430\u043a \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u044b\u0439 \u0441\u043f\u043e\u0441\u043e\u0431 \u043e\u0442\u0432\u043b\u0435\u0447\u044c\u0441\u044f \u0434\u043b\u044f \u043c\u043d\u043e\u0433\u0438\u0445 \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u0442\u0435\u043b\u0435\u0439.\n\n\u0414-\u0440 \u0411\u0440\u0430\u0439\u0442 \u043a\u0430\u043a\u0438\u043c-\u0442\u043e \u043e\u0431\u0440\u0430\u0437\u043e\u043c \u043f\u043e\u043b\u0443\u0447\u0438\u043b \u0434\u043e\u0441\u0442\u0443\u043f \u043a \u043b\u0438\u0447\u043d\u044b\u043c \u0434\u0435\u043b\u0430\u043c. \u041c\u044b \u043d\u0435 \u0437\u043d\u0430\u0435\u043c, \u043a\u0430\u043a \u044d\u0442\u043e \u043f\u0440\u043e\u0438\u0437\u043e\u0448\u043b\u043e, \u043d\u043e \u044d\u0442\u043e \u043e\u043f\u0440\u0435\u0434\u0435\u043b\u0451\u043d\u043d\u043e \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u043e.\n\n//\u041f\u0440\u0438\u043c\u0435\u0447\u0430\u043d\u0438\u0435 O5:// \u0414\u043e\u043a\u0442\u043e\u0440 \u0411\u0440\u0430\u0439\u0442 \u043d\u0435 \u0434\u043e\u043b\u0436\u0435\u043d \u0437\u043d\u0430\u0442\u044c \u043e \u0442\u043e\u043c, \u0447\u0442\u043e \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u0438\u043c \u0435\u0433\u043e \u043a\u043e\u043f\u0438\u044f\u043c \u0441\u043e\u0445\u0440\u0430\u043d\u0435\u043d\u0430 \u0436\u0438\u0437\u043d\u044c \u0434\u043b\u044f \u0440\u0430\u0431\u043e\u0442\u044b \u043d\u0430\u0434 \u043e\u0441\u043e\u0431\u044b\u043c\u0438 \u043f\u0440\u043e\u0435\u043a\u0442\u0430\u043c\u0438. \u0411\u0440\u0430\u0439\u0442\u044b 2-7 \u043d\u0430\u0437\u043d\u0430\u0447\u0435\u043d\u044b \u043d\u0430 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u0435 [\u0414\u0410\u041d\u041d\u042b\u0415 \u0423\u0414\u0410\u041b\u0415\u041d\u042b].\n\n**\u041f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0435:** \u0414\u043b\u044f \u043f\u043e\u043b\u043d\u043e\u0433\u043e \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u043b\u0435\u043d\u0438\u044f \u0441\u043e \u0432\u0441\u0435\u043c\u0438 \u043d\u0430\u0440\u0443\u0448\u0435\u043d\u0438\u044f\u043c\u0438, \u0441\u043e\u0432\u0435\u0440\u0448\u0451\u043d\u043d\u044b\u043c\u0438 \u0434-\u0440\u043e\u043c \u0411\u0440\u0430\u0439\u0442\u043e\u043c, \u043f\u043e\u0436\u0430\u043b\u0443\u0439\u0441\u0442\u0430, \u043f\u0440\u043e\u0441\u043c\u043e\u0442\u0440\u0438\u0442\u0435 \u043e\u0444\u0438\u0446\u0438\u0430\u043b\u044c\u043d\u044b\u0439 \u0441\u043f\u0438\u0441\u043e\u043a \u0442\u043e\u0433\u043e, [[[the-things-dr-bright-is-not-allowed-to-do-at-the-foundation|\u0447\u0442\u043e \u0434\u043e\u043a\u0442\u043e\u0440\u0443 \u0411\u0440\u0430\u0439\u0442\u0443 \u0437\u0430\u043f\u0440\u0435\u0449\u0430\u0435\u0442\u0441\u044f \u0434\u0435\u043b\u0430\u0442\u044c \u0432 \u0424\u043e\u043d\u0434\u0435]]]. \u0415\u0441\u043b\u0438 \u0431\u0443\u0434\u0443\u0442 \u043e\u0431\u043d\u0430\u0440\u0443\u0436\u0435\u043d\u044b \u043d\u043e\u0432\u044b\u0435 \u043d\u0430\u0440\u0443\u0448\u0435\u043d\u0438\u044f, \u043c\u043e\u0436\u0435\u0442\u0435 \u0441\u0432\u043e\u0431\u043e\u0434\u043d\u043e \u0438\u0445 \u0434\u043e\u0431\u0430\u0432\u0438\u0442\u044c. \u2013 O5-6.\n\n**\u041f\u0440\u0438\u043c\u0435\u0447\u0430\u043d\u0438\u0435:** //\u0423\u0443\u0443\u043a, \u0443\u0443\u043a \u0443\u043a\u043a \u0443\u0443\u043a, \u0438\u0438\u043a. \u0423\u0443\u0443\u043a, \u0438\u0438\u0438\u0438\u043a. \u0423\u0443\u0443\u043a \u0443\u0443\u043a \u0438\u0438\u043a \u0438\u0438\u043a, \u0443\u0443\u043a \u0443\u0443\u043a \u0443\u043a. \u0423\u0423\u0423\u0423\u0423\u0423K. \u0423\u0443\u0443\u043a. \u0423\u0443\u0443\u043a \u0443\u0443\u043a.// \u2013 MB.\n\n**\u041f\u0440\u0438\u043c\u0435\u0447\u0430\u043d\u0438\u0435:** //\u041a\u0430\u043a\u043e\u0433\u043e \u0447\u0451\u0440\u0442\u0430? \u0427\u0442\u043e \u0437\u0430 \u0445\u0443\u0439 \u044d\u0442\u043e \u043d\u0430\u043f\u0438\u0441\u0430\u043b? \u041f\u0440\u043e\u043a\u043b\u044f\u0442\u044c\u0435, \u044f \u0432\u043e\u0437\u043c\u0443\u0449\u0451\u043d \u044d\u0442\u0438\u043c\u0438 \u0432\u044b\u0432\u043e\u0434\u0430\u043c\u0438 \u043e \u043c\u043e\u0435\u0439 \u043c\u0430\u0442\u0435\u0440\u0438!// \u2013 \u0434-\u0440 \u0411\u0440\u0430\u0439\u0442.\n\n**\u041f\u0440\u0438\u043c\u0435\u0447\u0430\u043d\u0438\u0435:** \u0414\u043e\u043a\u0442\u043e\u0440 \u0411\u0440\u0430\u0439\u0442, \u0441\u043a\u0430\u0436\u0438\u0442\u0435, \u043a\u043e\u0433\u0434\u0430 \u043f\u0440\u043e\u0438\u0437\u043e\u0448\u043b\u0430 \u043f\u0435\u0440\u0432\u0430\u044f \u043f\u043e\u0434\u043e\u0431\u043d\u0430\u044f [[[http://www.scp-wiki.net/video-logs|\u0443\u0442\u0435\u0447\u043a\u0430 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0438]]]? \u042d\u0442\u043e \u043c\u043e\u0436\u0435\u0442 \u0441\u0442\u0430\u0442\u044c \u0441\u0435\u0440\u044c\u0451\u0437\u043d\u043e\u0439 \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u043e\u0439\u2026 - O5-12\n\n[[div_ class=\"rimg\"]]\n[[image drbunny width=\"320\" ]]\n[[span]]\u041e\u0431 \u044d\u0442\u043e\u0439 \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u0438\u0438 \u0443 \u043d\u0430\u0441 \u0433\u043e\u0432\u043e\u0440\u0438\u0442\u044c \u043d\u0435 \u043f\u0440\u0438\u043d\u044f\u0442\u043e[[/span]]\n[[/div]]\n", "tags": ["\u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0430:\u043b\u0438\u0447\u043d\u043e\u0435_\u0434\u0435\u043b\u043e", "\u0444\u0438\u043b\u0438\u0430\u043b:en"], "parent": null, "locked": false}